msgid "In order to successfully use &kfind;, you need &kde; 2.1."
msgstr ""

#: index.docbook:300
msgid "Compilation and Installation"
msgstr ""

#: index.docbook:302
msgid "In order to compile and install &kfind; on your system, type the following in the base directory of the kdeutils distribution:"
msgstr ""

#: index.docbook:307
msgid ""
      "<prompt>%</prompt> <userinput><command>./configure</command></userinput>\n"
      "<prompt>%</prompt> <userinput><command>make</command></userinput>\n"
      "<prompt>%</prompt> <userinput><command>make</command> <option>install</option></userinput>"
msgstr ""

#: index.docbook:313
msgid "Since &kfind; uses <command>autoconf</command> and <command>automake</command> you should have no trouble compiling it. Should you run into problems please report them to the &kde; mailing lists."
msgstr ""

