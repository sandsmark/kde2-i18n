msgid	"Freehand lines"
msgstr	""

#: index.docbook:615
msgid	"<link linkend=\"li\">Lines</link>, <link "
	"linkend=\"pf\">Polylines</link>, <link linkend=\"gp\">Polygons</link>"
msgstr	""

