#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Změnit heslo tohoto uživatele."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Změní UNIXové heslo."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Správce"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Vaše heslo bylo změněno."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Změnit heslo"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Prosím zadejte své současné heslo."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Komunikace s programem 'passwd' selhala."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Nelze nalézt program 'passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Neplatné heslo! Prosím zkuste znovu."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Interní chyba: neplatná návratová hodnota z PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Prosím zadejte své nové heslo."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Vaše heslo bude oříznuto na 8 znaků."

