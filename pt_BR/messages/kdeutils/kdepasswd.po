#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Mudar senha desse usuário."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Muda uma senha Unix."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Mantenedor"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Sua senha foi mudada."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Mudar senha"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Digite sua senha atual"

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Conversação com 'passwd' falhou."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Impossível encontrar o programa 'passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Senha incorreta! Tente novamente."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Erro interno: valor de retorno ilegal de PasswdProcess:checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Digite sua nova senha."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Sua senha será truncada em 8 caracteres."

