#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Изменить пароль этого пользователя."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Изменяет пароль UNIX."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Текущее сопровождение"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Ваш пароль изменен."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Изменить пароль"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Введите ваш текущий пароль."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Не удалось запустить 'passwd'."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Не могу найти программу 'passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Неверный пароль! Попробуйте еще раз."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Внутренняя ошибка: функция "
"PasswdProcess::checkCurrent возвратила недопустимое "
"значение."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Введите ваш новый пароль."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Ваш пароль будет укорочен до 8 символов."

