msgid "<sgmltag class=\"element\">KugarData</sgmltag> Document Type Definition"
msgstr ""
"<sgmltag class=\"element\">Данные Kugar</sgmltag> "
"определение типа документа (DTD)"

#: index.docbook:10
msgid ""
"&lt;?xml version=\"1.0\" encoding=\"UTF-8\"?&gt;\n"
"\n"
"&lt;!DOCTYPE KugarData [\n"
"     &lt;!ELEMENT KugarData (Row* )&gt;\n"
"     &lt;!ATTLIST KugarData\n"
"         Template  CDATA  #REQUIRED&gt;\n"
"\n"
"     &lt;!ELEMENT Row EMPTY&gt;\n"
"     &lt;!ATTLIST Row\n"
"         col1  CDATA  #REQUIRED\n"
"         col2  CDATA  #REQUIRED\n"
"         ...   CDATA  #REQUIRED\n"
"         coln  CDATA  #REQUIRED&gt;\n"
" ]&gt;"
