#: rc.cpp:1
msgid "HTML Export Dialog"
msgstr "Настройка экспорта в HTML"

#: rc.cpp:2
msgid "Document Type"
msgstr "Тип документа"

#: rc.cpp:3
msgid "&HTML 4.01"
msgstr "&HTML 4.01"

#: rc.cpp:4
msgid "&XHTML 1.0"
msgstr "&XHTML 1.0"

#: rc.cpp:5
msgid "Mode"
msgstr "Форматирование"

#: rc.cpp:6
msgid "S&partan (Only document structure, no formatting!)"
msgstr ""
"&Нет (только структура документа без "
"форматирования)"

#: rc.cpp:7
msgid "&Direct (Formatting coded using HTML, no style sheets)"
msgstr ""
"&Чистый HTML (использование HTML без таблиц "
"стилей)"

#: rc.cpp:8
msgid "&Style (Formatting coded using CSS2, no style sheets)"
msgstr ""
"&Со стилями (использование CSS2 без внешних "
"таблиц стилей)"

#: rc.cpp:9
msgid "&Full Power (As much as possible using CSS2, less compatible)"
msgstr ""
"&Полное (По возможности использование CSS2, "
"возможна несовместимость)"

#: rc.cpp:10
msgid "HTML Import Dialog"
msgstr "Настройки экспорта в HTML"

#: rc.cpp:11
msgid "Character Set"
msgstr "Кодировка"

#: rc.cpp:12
msgid "Hint For Character Set:"
msgstr "Выбор кодировки:"

#: rc.cpp:13
msgid "&Hint"
msgstr "&Спрашивать всегда"

#: rc.cpp:14
msgid "Use hint only as &fall back!"
msgstr "Спрашивать только при &ошибках"

#: rc.cpp:15
msgid "Force the use of the hint! (For buggy files!)"
msgstr ""
"Не проверять ошибки перекодировки (для "
"сложных файлов)"

#: rc.cpp:16
msgid "Do &not use the hint! (Recommanded!)"
msgstr "&Автоматически (рекомендуется)"
