msgstr "Заголовок задания для печати"

#: main.cpp:31
msgid "Printer option"
msgstr "Свойства принтера"

#: main.cpp:32
msgid "Job output mode (gui, console, none)"
msgstr ""
"Режим вывода задания (графический, "
"консольный, нет)"

#: main.cpp:33
msgid "Print system to use (lpd, cups)"
msgstr ""
"Какую подсистему печати использовать (lpd, "
"cups)"

#: main.cpp:34
msgid "Forbid impression from STDIN"
msgstr "Запретить печать со STDIN"

#: main.cpp:35
msgid "Files to load"
msgstr "Открыть файлы"

#: main.cpp:41
msgid "A printer tool for KDE"
msgstr "Утилита управления принтером для KDE"

#: printwrapper.cpp:51
msgid "Print information"
msgstr "Печатать информационные сообщения"

#: printwrapper.cpp:52 printwrapper.cpp:59
msgid "Print warning"
msgstr "Печатать предупреждения"

#: printwrapper.cpp:53 printwrapper.cpp:59
msgid "Print error"
msgstr "Печатать ошибки"

#: printwrapper.cpp:59
msgid "Print info"
msgstr "Печатать информацию"

#: printwrapper.cpp:136
msgid "\"%1\": file not found"
msgstr "\"%1\": файл не найден"

#: printwrapper.cpp:148
msgid "Can't print from STDIN (do not use '--nostdin' option)"
