#: rc.cpp:1
msgid "HTML Export Dialog"
msgstr "Pogovorno okno za izvoz HTML"

#: rc.cpp:2
msgid "Document Type"
msgstr "Vrsta dokumenta"

#: rc.cpp:3
msgid "&HTML 4.01"
msgstr "&HTML 4.01"

#: rc.cpp:4
msgid "&XHTML 1.0"
msgstr "&XHTML 1.0"

#: rc.cpp:5
msgid "Mode"
msgstr "Način"

#: rc.cpp:6
msgid "S&partan (Only document structure, no formatting!)"
msgstr "Š&partanski (le struktura dokumenta, brez formatiranja!)"

#: rc.cpp:7
msgid "&Direct (Formatting coded using HTML, no style sheets)"
msgstr "&Direktni (formatiranje z uporabo HTML, brez slogovnih predlog)"

#: rc.cpp:8
msgid "&Style (Formatting coded using CSS2, no style sheets)"
msgstr "&Slog (formatiranje z uporabo CSS2, brez slogovnih predlog)"

#: rc.cpp:9
msgid "&Full Power (As much as possible using CSS2, less compatible)"
msgstr "&Polno zmogljivo (toliko, kot dopušča CSS2, manj združljivo)"

#: rc.cpp:10
msgid "HTML Import Dialog"
msgstr "Pogovorno okno za uvoz HTML"

#: rc.cpp:11
msgid "Character Set"
msgstr "Nabor znakov"

#: rc.cpp:12
msgid "Hint For Character Set:"
msgstr "Namig za nabor znakov:"

#: rc.cpp:13
msgid "&Hint"
msgstr "Na&mig"

#: rc.cpp:14
msgid "Use hint only as &fall back!"
msgstr "Namig uporabi le kot &rezervo!"

#: rc.cpp:15
msgid "Force the use of the hint! (For buggy files!)"
msgstr "Uveljavi uporabo namiga! (za hroščate datoteke!)"

#: rc.cpp:16
msgid "Do &not use the hint! (Recommanded!)"
msgstr "&Ne uporabljaj namiga! (priporočeno!)"
