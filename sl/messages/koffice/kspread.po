msgstr "Funkcija IMSUM() vrne seštevek večih kompleksnih števil v obliki x+yi."

#: xml_doc.cc:543
msgid "IMSUM(value;value;...)"
msgstr "IMSUM(vrednosti;vrednost;...)"

#: xml_doc.cc:544
msgid "IMSUM(1.2;\"3.4+5i\") returns \"4.6+5i\""
msgstr "IMSUM(1,2;\"3,4+5i\") vrne \"4,6+5i\""

#: xml_doc.cc:545
msgid "IMSUM(1.2;\"1i\") returns \"1.2+i\""
msgstr "IMSUM(1,2;\"1i\") vrne \"1,2+i\""

#: xml_doc.cc:551
msgid "The IMDIV() returns the division of several complex numbers of form x+yi."
msgstr "Funkcija IMDIV() vrne deljenje večih kompleksnih števil v obliki x+yi."

#: xml_doc.cc:552
msgid "IMDIV(value;value;...)"
msgstr "IMDIV(vrednosti;vrednost;...)"

#: xml_doc.cc:553
msgid "IMDIV(1.2;\"3.4+5i\") returns \"0.111597-0.164114i\""
msgstr "IMDIV(1,2;\"3,4+5i\") vrne \"0,111597-0,164114i\""

#: xml_doc.cc:554
msgid "IMDIV(\"12+i\";\"12-i\") returns \"0.986207+0.16551i\""
msgstr "IMDIV(\"12+i\";\"12-i\") vrne \"0,986207+0,16551i\""

#: xml_doc.cc:560
msgid "The IMSUB() returns the difference of several complex numbers of form x+yi."
msgstr "Funkcija IMUB() vrne razliko večih kompleksnih števil v obliki x+yi."

#: xml_doc.cc:561
msgid "IMSUB(value;value;...)"
msgstr "IMSUB(vrednost;vrednost;...)"

#: xml_doc.cc:562
msgid "IMSUB(1.2;\"3.4+5i\") returns \"-2.2-5i\""
msgstr "IMSUB(1,2;\"3,4+5i\") vrne \"-2,2-5i\""

#: xml_doc.cc:563
msgid "IMSUB(1.2;\"1i\") returns \"1.2-i\""
msgstr "IMSUB(1,2;\"1i\") vrne \"1,2-i\""

#: xml_doc.cc:569
msgid "The IMPRODUCT() returns the product of several complex numbers of form x+yi."
msgstr "Funkcija IMPRODUCT() vrne razliko večih kompleksnih števil v obliki x+yi."

#: xml_doc.cc:570
msgid "IMPRODUCT(value;value;...)"
msgstr "IMPRODUCT(vrednost;vrednost;...)"

#: xml_doc.cc:571
msgid "IMPRODUCT(1.2;\"3.4+5i\") returns \"4.08+6i\""
msgstr "IMPRODUCT(1,2;\"3,4+5i\") vrne \"4,08+6i\""

#: xml_doc.cc:572
msgid "IMPRODUCT(1.2;\"1i\") returns \"+1.2i\""
msgstr "IMPRODUCT(1,2;\"1i\") returns \"+1,2i\""

#: xml_doc.cc:574
msgid ""
"The IMCONJUGATE(complex number) returns the conjugate of a complex number of "
"form x+yi."
msgstr ""
"Funkcija INCONJUGATE(kompleksno število) vrne konjugirano vrednost "
"kompleksnega števila v obliki x+yi."

#: xml_doc.cc:575
msgid "IMCONJUGATE(complex number)"
msgstr "IMCONJUGATE(kompleksno število)"

#: xml_doc.cc:576
msgid "IMCONJUGATE(\"1.2+5i\") returns \"1.2-5i\""
msgstr "IMCONJUGATE(\"1,2+5i\") vrne \"1,2-5i\""

#: xml_doc.cc:577
msgid "IMCONJUGATE(\"-i\") returns \"i\""
msgstr "IMCONJUGATE(\"-i\") vrne \"i\""

#: xml_doc.cc:578
msgid "IMCONJUGATE(\"12\") returns \"12\""
msgstr "IMCONJUGATE(\"12\") vrne \"12\""

#: xml_doc.cc:580
msgid ""
"The IMARGUMENT(complex number) returns the argument of a complex number of "
"form x+yi."
msgstr ""
"Funkcija IMARGUMENT(kompleksno število) vrne argument kompleksnega števila "
"v obliki x+yi."

#: xml_doc.cc:581
msgid "IMARGUMENT(complex number)"
msgstr "IMARGUMENT(kompleksno število)"

#: xml_doc.cc:582
msgid "IMARGUMENT(\"1.2+5i\") returns 0.6072"
msgstr "IMARGUMENT(\"1,2+5i\") vrne 0,6072"

#: xml_doc.cc:583
msgid "IMARGUMENT(\"-i\") returns -1.57079633 "
msgstr "IMARGUMENT(\"-i\") vrne -1,57079633 "

#: xml_doc.cc:584
msgid "IMARGUMENT(\"12\") returns \"#Div/0\""
msgstr "IMARGUMENT(\"12\") vrne \"#Del/0\""

