#: main.cpp:45
msgid "KFM Exec - Opens remote files, watches modifications, asks for upload"
msgstr "KFM Exec - Odpira oddaljene datoteke, spremlja spremembe, ureja prenose"

#: main.cpp:51
msgid "Command to execute."
msgstr "Ukaz, ki naj se izvede."

#: main.cpp:52
msgid "URL(s) or local file used for 'command'."
msgstr "URL(-ji) ali lokalna datoteka, ki naj se uporabi za ,ukaz`."

#: main.cpp:68
msgid ""
"'command' expected.\n"
msgstr ""
"Pričakovan ,ukaz`.\n"

#: main.cpp:89
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"URL %1\n"
"je zmaličen."

#: main.cpp:196
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"Datoteka\n"
"%1\n"
"je bila spremenjena.\n"
"Ali želite poslati spremembe?"

#: main.cpp:197
msgid "File changed"
msgstr "Datoteka spremenjena"

#: main.cpp:256
msgid "KFMExec"
msgstr "KFMExec"

#: main.cpp:271
msgid ""
"Syntax Error:\n"
"kfmexec command [URLs ....]"
msgstr ""
"Skladenjska napaka:\n"
"kfmexec ukaz [URLji ....]"

