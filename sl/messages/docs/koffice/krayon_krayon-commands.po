msgid ""
"Changes the cursor to a <guilabel>What's This?</guilabel> cursor. When this "
"cursor is clicked on an item, icon or other control, a quick help panel is "
"displayed."
msgstr ""

#: index.docbook:1102
msgid "<guimenu>Help</guimenu> <guimenuitem>Report Bug...</guimenuitem>"
msgstr "<guimenu>Pomoč</guimenu> <guimenuitem>Poročaj o hrošču...</guimenuitem>"

