"te datoteke."

#: index.docbook:425
msgid "<guimenu>Settings</guimenu> <guimenuitem>Word Separators</guimenuitem>"
msgstr "<guimenu>Nastavitve</guimenu> <guimenuitem>Ločila besed</guimenuitem>"

