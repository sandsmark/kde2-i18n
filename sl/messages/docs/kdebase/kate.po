msgstr "Označi besedilo en znak na levo"

#: index.docbook:366
msgid "<keycap>Shift</keycap><keycap>Right Arrow</keycap>"
msgstr "<keycap>Shift</keycap><keycap>Desna puščica</keycap>"

#: index.docbook:366
msgid "Mark text one character to the right"
msgstr "Označi besedilo en znak na desno"

#: index.docbook:366
msgid "<keycap>F1</keycap>"
msgstr "<keycap>F1</keycap>"

#: index.docbook:366
msgid "Help"
msgstr "Pomoč"

