"keycode 89 = 3\n"
"\n"
"keycode 90 = 0\n"
"keycode 91 = comma\n"
"keycode 86 = plus\n"
"\n"
"! deactivates NumLock key\n"
"keycode 77 ="
msgstr ""
"! redefines numeric keypad to be used without NumLock\n"
