"keycode 79 = 7\n"
"keycode 80 = 8\n"
"keycode 81 = 9\n"
"\n"
"keycode 83 = 4\n"
"keycode 84 = 5\n"
"keycode 85 = 6\n"
"\n"
"keycode 87 = 1\n"
