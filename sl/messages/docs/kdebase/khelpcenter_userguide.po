#: index.docbook:10
msgid "K Desktop Environment"
msgstr "Namizno okolje K (K Desktop Environment)"

#: index.docbook:12 index.docbook:23
msgid "The KDE Team"
msgstr "Ekipa KDE"

#: index.docbook:15
msgid "ROLES_OF_TRANSLATORS"
msgstr ""
"<othercredit "
"role=\"translator\"><firstname>Andrej</firstname><surname>Vernekar</surname>"
"<affiliation><address><email>andrej.vernekar@kiss.uni-lj.si</email></address>"
