msgstr "<member>CDE</member>"

#: index.docbook:100
msgid "Dark Blue,"
msgstr "Donkerblauw,"

#: index.docbook:100
msgid "Desert red,"
msgstr "Woestijnrood,"

#: index.docbook:100
msgid "Digital CDE,"
msgstr "Digital CDE,"

#: index.docbook:100
msgid "Pale gray,"
msgstr "Vaalgrijs,"

#: index.docbook:100
