#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr "KDE-programma voor het openen van URL's vanaf de commandoregel"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Beschikbare commando's weergeven."

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Commando (zie --commando's)."

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Argumenten voor commando."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Gebruik:\n"

#: kfmclient.cc:77
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"   kfmclient openURL 'url' ['bestandstype']\n"
"            # opent een venster met het 'url-adres'.\n"
"            #  'url' kan een relatief pad zijn\n"
"            #   of een bestandsnaam, zoals . of submap/\n"
"            #   Als 'url' wordt weggelaten, dan wordt $HOME gebruikt.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # If 'bestandstype' is opgegeven, dan zal die gebruikt worden om "
"te bepalen welk \n"
"            #   component konqueror zal gebruiken. Bijvoorbeeld, zet het op\n"
"            #   text/html voor een webpagina, om deze sneller te openen.\n"
"\n"

#: kfmclient.cc:86
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profiel' ['URL-adres]\n"
"            # Opent een venster met het gegeven profiel.\n"
"            #   'profiel' is een bestand uit "
"~/.kde/share/apps/konqueror/profiles.\n"
"           #   'URL-adres' is een optioneel te openen URL-adres.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"            # opent een menu met de eigenschappen.\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Probeert 'url' uit te voeren. 'url' kan een gebruikelijke\n"
"            #   URL-adres zijn, dit URL-adres wordt dan geopend. U mag\n"
"            #   'binding' weglaten. In dat geval wordt de standaard-binding\n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   gebruikt. 'URL' mag uiteraard het URL-adres van een\n"
"            #   document of een  *.desktop-bestand zijn.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   Op deze manier kunt u bijvoorbeeld een bestandssysteem "
"aankoppelen\n"
"            #   door 'Mount default' in te stellen als binding voor \n"
"            #   'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient move 'src' 'dest'\n"
"            # Verplaatst het URL-adres van het vertrekpunt ('src') naar de "
"bestemming ('dest').\n"
"            #   'src' mag een lijst met URL-adressen zijn.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient copy 'src' 'dest'\n"
"            # kopiÃ«ert een URL-adres van het vertrekpunt ('src') naar de "
"bestemming ('dest').\n"
"            #   'src' mag een lijst met URL-adressen zijn.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"            # Herschikt alle pictogrammen op het bureaublad.\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"            # Herleest de configuratie van Konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"            # Herleest de configuratie van kdesktop.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Voorbeelden:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // koppelt de cd-rom aan het bestandssysteem\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/rinse/data/test.html\n"
"             // opent een bestand met de standaard-applicatiebinding\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/rinse/data/test.html Netscape\n"
"             // opent het bestand met Netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // opent een nieuw venster met dit URL-adres\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // start emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // opent de map van de cd-rom\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // opent de huidige map. Erg handig.\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"Profiel %1 werd niet gevonden\n"

#: kfmclient.cc:290
msgid ""
"Syntax Error: Not enough arguments\n"
msgstr ""
"Syntaxisfout: te weinig argumenten\n"

#: kfmclient.cc:295
msgid ""
"Syntax Error: Too many arguments\n"
msgstr ""
"Syntaxisfout: teveel argumenten\n"

#: kfmclient.cc:417
msgid ""
"Syntax Error: Unknown command '%1'\n"
msgstr ""
"Syntaxisfout: '%1' is een onbekend commando\n"

