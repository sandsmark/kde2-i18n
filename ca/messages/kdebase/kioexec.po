#: main.cpp:45
msgid "KFM Exec - Opens remote files, watches modifications, asks for upload"
msgstr "KFM Exec - Obri fitxers remots, vigila modificacions, demana fer la càrrega"

#: main.cpp:51
msgid "Command to execute."
msgstr "Comanda a executar."

#: main.cpp:52
msgid "URL(s) or local file used for 'command'."
msgstr "URL(s) o fitxers locals usats per a 'comanda'."

#: main.cpp:68
msgid ""
"'command' expected.\n"
msgstr ""
"S'espera 'comanda'.\n"

#: main.cpp:89
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"La URL %1\n"
"és incorrecta"

#: main.cpp:196
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"El fitxer\n"
"%1\n"
"ha estat modificat.\n"
"Voleu actualitzar els canvis?"

#: main.cpp:197
msgid "File changed"
msgstr "El fitxer ha canviat"

#: main.cpp:256
msgid "KFMExec"
msgstr "KFMExec"

#: main.cpp:271
msgid ""
"Syntax Error:\n"
"kfmexec command [URLs ....]"
msgstr ""
"Error de sintàxi:\n"
"comanda kfmexec [URLs ....]"

