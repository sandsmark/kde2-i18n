msgstr "<entry> 15 F</entry>"

#: index.docbook:237
msgid "<entry> $20</entry>"
msgstr "<entry> 20 F</entry>"

#: index.docbook:237
msgid "<entry> $25</entry>"
msgstr "<entry> 25 F</entry>"

