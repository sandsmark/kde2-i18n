msgid "RedHat 5.2 (Hagen Höpfner)"
msgstr "RedHat 5.2 (Hagen Höpfner)"

#: index.docbook:174
msgid "SuSE 6.0 (Hagen Höpfner)"
msgstr "SuSE 6.0 (Hagen Höpfner)"

#: index.docbook:174
msgid "SuSE 6.1 Cebit Beta (Hagen Höpfner)"
msgstr "SuSE 6.1 Cebit Beta (Hagen Höpfner)"

#: index.docbook:174
msgid "SuSE 6.1 (Hagen Höpfner)"
msgstr "SuSE 6.1 (Hagen Höpfner)"

#: index.docbook:174
msgid "SuSE 6.2 (Joachim Ansorg)"
msgstr "SuSE 6.2 (Joachim Ansorg) "

