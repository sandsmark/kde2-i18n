#: index.docbook:10
msgid "K Desktop Environment"
msgstr "L'environnement de bureau KDE"

#: index.docbook:12 index.docbook:23
msgid "The KDE Team"
msgstr "L'équipe KDE"

#: index.docbook:15
msgid "ROLES_OF_TRANSLATORS"
msgstr ""
"<othercredit "
"role=\"translator\"><firstname>Anne-Marie</firstname><surname>"
"Mahfouf</surname><affiliation><address><email>a-m.mahfouf@lineone.net</email>"
