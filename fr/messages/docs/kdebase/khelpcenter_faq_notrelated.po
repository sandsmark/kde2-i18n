msgstr ""
"! redéfinit le clavier numérique pour l'utiliser sans Verr Num\n"
"keycode 79 = 7\n"
"keycode 80 = 8\n"
"keycode 81 = 9\n"
"\n"
"keycode 83 = 4\n"
