msgid ""
"<shortcut> <keycombo> <keycap>PgDown</keycap> </keycombo> </shortcut> "
"<guimenu>Go</guimenu> <guimenuitem>Next</guimenuitem>"
msgstr ""
"<shortcut> <keycombo> <keycap>Page suiv</keycap> </keycombo> </shortcut> "
"<guimenu>Aller</guimenu> <guimenuitem>Suivant</guimenuitem>"

