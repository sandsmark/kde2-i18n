msgid "Home"
msgstr "Maison"

#: ../common/newsengine.cpp:77
msgid "Recreation"
msgstr "Loisirs"

#: ../common/newsengine.cpp:78
msgid "Reference"
msgstr "Références"

#: ../common/newsengine.cpp:79
msgid "Science"
msgstr "Sciences"

#: ../common/newsengine.cpp:80
msgid "Shopping"
msgstr "Achats"

#: ../common/newsengine.cpp:81
msgid "Society"
msgstr "Société"

