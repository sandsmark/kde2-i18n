#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Modification du mot de passe de cet utilisateur."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "Modificateur de mot de passe pour KDE"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Change un mot de passe Unix."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Mainteneur"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Votre mot de passe a été changé."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Changer le mot de passe"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Veuillez saisir votre mot de passe actuel."

