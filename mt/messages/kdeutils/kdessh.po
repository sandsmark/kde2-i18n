msgstr "Speċifika fejn qiegħed l-istub fuq il-kompjuter remot"

#: kdessh.cpp:44
msgid "Do not keep password."
msgstr "Iżżommx il-password"

#: kdessh.cpp:45
msgid "Stop the daemon (forgets all passwords)."
msgstr "Waqqaf id-daemon (insa' l-passwords kollha)"

#: kdessh.cpp:46
msgid "Enable terminal output (no password keeping)."
msgstr "Ippermetti l-użu ta' terminal (ma jżommx il-password)"

#: kdessh.cpp:53
msgid "KDE ssh"
msgstr "SSH għall-KDE"

#: kdessh.cpp:54
msgid "Runs a program on a remote host."
msgstr "Iħaddem programm fuq kompjuter remot."

#: kdessh.cpp:57
msgid "Maintainer"
msgstr "Mantenitur"

#: kdessh.cpp:85
msgid "No command or host specified!"
msgstr "Ebda kmand jew kompjuter speċifikat."

