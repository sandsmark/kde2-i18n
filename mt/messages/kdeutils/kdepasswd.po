#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Ibdel il-password ta' dal-user."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Tibdel il-password tal-Unix"

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Mantenitur"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Il-password tiegħek ġiet mibdula."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Ibdel password"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Jekk jogħġbok daħħal il-password kurrenti."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Komunikazzjoni ma' \"passwd\" falliet."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Ma setax jinstab il-programm \"passwd\"."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Password ħażina. Jekk jogħġbok erġa pprova."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Problema interna: valur ħażin ritornat minn PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Jekk jogħġbok daħħal il-password il-ġdid."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Il-password tiegħek tiġi mqassra għal 8 ittri."

