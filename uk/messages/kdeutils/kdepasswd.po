#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Змінити пароль користувача."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Змінити пароль Unix."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Супровід"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Ваш пароль змінено."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Змінити пароль"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Будь ласка, введіть ваш поточний пароль."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Помилка спілкування з `passwd'."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Не можу знайти програму `passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Невірний пароль! Спробуйте ще раз."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Внутрішня помилка: невірне значення "
"відповіді від PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Будь ласка, введіть новий пароль."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Ваш пароль буде врізано до 8 символів."

