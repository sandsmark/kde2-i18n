"вищепоказаний список;"

#: ksmboptdlg.cpp:378
msgid "server: "
msgstr "сервер:"

#: ksmboptdlg.cpp:379
msgid "share: "
msgstr "спільний ресурс:"

#: ksmboptdlg.cpp:380
msgid "login: "
msgstr "ім'я:"

#: ksmboptdlg.cpp:381
