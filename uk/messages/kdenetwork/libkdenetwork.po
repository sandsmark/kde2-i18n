msgstr "PGP Версія &2.x"

#: kpgp.cpp:1141
msgid "PGP Version &5.x"
msgstr "PGP Версія &5.x"

#: kpgp.cpp:1145
msgid "PGP Version &6.x"
msgstr "PGP Версія &6.x"

#: kpgp.cpp:1149
