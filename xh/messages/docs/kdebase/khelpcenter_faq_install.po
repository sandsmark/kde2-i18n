#~ msgid "Here's the FixMakesSq script:"
#~ msgstr "Nalu ushicilelo lwe FixMakesSq:"

#~ msgid ""
#~ "#!/bin/perl -ni.bak\n"
#~ " \n"
#~ "if ((/-lpng/) || (/-ljpeg/))\n"
#~ "{\n"
#~ "    if (/^(.*)-lpng(.*)$/)\n"
#~ "    {\n"
#~ "        $b4 = $1;\n"
#~ "        $af = $2;\n"
#~ "        print \"$b4 /path/to/KDE/libs/libpng.a $af\\n\";\n"
#~ "    }\n"
#~ "    if (/^(.*)-ljpeg(.*)$/)\n"
#~ "    {\n"
#~ "        $b4 = $1;\n"
#~ "        $af = $2;\n"
#~ "        print \"$b4 /path/to/KDE/libs/libjpeg.a $af\\n\";\n"
#~ "    }\n"
#~ "}\n"
#~ "        \n"
#~ "#NOTE!! Uncomment this section if you want to add\n"
#~ "#-D_LANGUAGE_C_PLUS_PLUS to all \n"
#~ "#makefiles.  I personally have not tried this, but it should not break \n"
#~ "#anything.\n"
#~ "# Alternately, add the flag in the places noted below.\n"
#~ "\n"
#~ "#elsif (/cxxflags\\s*=(.*)$/i)\n"
#~ "#{\n"
#~ "#    print \"CXXFLAGS =  -D_LANGUAGE_C_PLUS_PLUS $1\\n\";\n"
#~ "#}\n"
#~ "else\n"
#~ "{\n"
#~ "    print;\n"
#~ "}"
#~ msgstr ""
#~ "#!/bin/perl -ni.bak\n"
#~ " \n"
#~ "if ((/-lpng/) || (/-ljpeg/))\n"
#~ "{\n"
#~ "    if (/^(.*)-lpng(.*)$/)\n"
#~ "    {\n"
#~ "        $b4 = $1;\n"
#~ "        $af = $2;\n"
#~ "        print \"$b4 /path/to/KDE/libs/libpng.a $af\\n\";\n"
#~ "    }\n"
#~ "    if (/^(.*)-ljpeg(.*)$/)\n"
#~ "    {\n"
#~ "        $b4 = $1;\n"
#~ "        $af = $2;\n"
#~ "        print \"$b4 /path/to/KDE/libs/libjpeg.a $af\\n\";\n"
#~ "    }\n"
#~ "}\n"
#~ "        \n"
#~ "#QAPHELA!! SUKUGQABAZA ELI CANDELO UKUBA UFUNA UKUDIBANISA\n"
#~ "#-D_LANGUAGE_C_PLUS_PLUS kuzo zonke \n"
#~ "#iifayile zokwenza.  Mna ngokwam andikuzamanga oku, kodwa ayinakwaphuka "
#~ "noko \n"
#~ "#nayiphi na into.\n"
