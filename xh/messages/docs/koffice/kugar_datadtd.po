"     &lt;!ATTLIST Row\n"
"         col1  CDATA  #REQUIRED\n"
"         col2  CDATA  #REQUIRED\n"
"         ...   CDATA  #REQUIRED\n"
"         coln  CDATA  #REQUIRED&gt;\n"
" ]&gt;"
