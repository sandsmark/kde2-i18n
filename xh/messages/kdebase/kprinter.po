#: main.cpp:29
msgid "Printer/destination to print on"
msgstr "Umshicileli/indawo engqonileyo yokushicilela"

#: main.cpp:30
msgid "Title for the print job"
msgstr "Iwonga lomsebenzi woshicilelo"

#: main.cpp:31
msgid "Printer option"
msgstr "Ukhetho lomshicileli"

#: main.cpp:32
msgid "Job output mode (gui, console, none)"
msgstr ""
"Indlela yomsebenzi wegalelo langaphandle (gui, isakhelo equlathe ibhodo "
"enezitshixo, akukho nanye)"

#: main.cpp:33
msgid "Print system to use (lpd, cups)"
msgstr "Shicilela indlela ozakuyisebenzisa (Ipd, iicup)"

#: main.cpp:34
msgid "Forbid impression from STDIN"
msgstr "Khusela ushicilelo kwakhona kwi STDIN"

#: main.cpp:35
msgid "Files to load"
msgstr "Iifayile zokulayishwa"

#: main.cpp:41
msgid "A printer tool for KDE"
msgstr "Isixhobo somshicileli we KDE"

#: printwrapper.cpp:51
msgid "Print information"
msgstr "Shicilela ulwazi"

#: printwrapper.cpp:52 printwrapper.cpp:59
msgid "Print warning"
msgstr "Shicilela isilumkiso"

#: printwrapper.cpp:53 printwrapper.cpp:59
msgid "Print error"
msgstr "Shicilela imposiso"

#: printwrapper.cpp:59
msgid "Print info"
msgstr "Shicilela ulwazi"

#: printwrapper.cpp:136
msgid "\"%1\": file not found"
msgstr "\"%1\": ifayile ayifumaneki"

#: printwrapper.cpp:148
msgid "Can't print from STDIN (do not use '--nostdin' option)"
msgstr "Ayikwazi ukushicilela kwi STDIN (sukuyisebenzisa '--nostdin' ukhetho)"

#: printwrapper.cpp:184
msgid "Unable to open temporary file."
msgstr "Ayikwazi ukuvula ifayile yethutyana."

#: printwrapper.cpp:194
msgid "Stdin is empty, no job sent."
msgstr "iStdin ayinanto, akukho msebenzi othunyelweyo."

#: printwrapper.cpp:200
msgid "Error while printing files"
msgstr "Imposiso ngexesha kushicilelwa iifayile"

#: printwrapper.cpp:203
msgid "<nobr>File(s) sent to printer <b>%1</b>.</nobr>"
msgstr "<nobr>Ifayile(ii) ithunyelwe kumshicileli <b>%1</b> </nobr>"
