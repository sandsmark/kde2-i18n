#: rc.cpp:1
msgid "HTML Export Dialog"
msgstr "HTML Incoko yababini yorhwebo lwangaphandle"

#: rc.cpp:2
msgid "Document Type"
msgstr "Udidi loxwebhu"

#: rc.cpp:3
msgid "&HTML 4.01"
msgstr "&HTML 4.01"

#: rc.cpp:4
msgid "&XHTML 1.0"
msgstr "&XHTML 1.0"

#: rc.cpp:5
msgid "Mode"
msgstr "Indlela"

#: rc.cpp:6
msgid "S&partan (Only document structure, no formatting!)"
msgstr "Eyo&khalipho (Isakhiwo soxwebhu kuphela, akukho lufomato!)"

#: rc.cpp:7
msgid "&Direct (Formatting coded using HTML, no style sheets)"
msgstr "&Ngqo (Ufomato olunekhowudi usebenzisa i HTML, akukho makhasi ohlobo)"

#: rc.cpp:8
msgid "&Style (Formatting coded using CSS2, no style sheets)"
msgstr "&Uhlobo (Ufomato olunekhowudi usebenzisa i CSS2, akukho makhasi ohlobo)"

#: rc.cpp:9
msgid "&Full Power (As much as possible using CSS2, less compatible)"
msgstr ""
"&Amandla apheleleyo (Ngokuninzi okunokwenzeka usebenzisa i CSS2, engephantsi "
"ngophikiso)"

#: rc.cpp:10
msgid "HTML Import Dialog"
msgstr "HTML Incoko yababini erhwetyelwe ngaphakathi"

#: rc.cpp:11
msgid "Character Set"
msgstr "Ucwangciso lomsebenzi"

#: rc.cpp:12
msgid "Hint For Character Set:"
msgstr "Ukuthi thsuphe kweqela labasebenzi:"

#: rc.cpp:13
msgid "&Hint"
msgstr "&Ukuthi tshuphe"

#: rc.cpp:14
msgid "Use hint only as &fall back!"
msgstr "Sebenzisa ukuthi tshuphe kuphela njenge&siwo sasemva!"

#: rc.cpp:15
msgid "Force the use of the hint! (For buggy files!)"
msgstr "Nyanzela usebenziso lokuthi tshuphe! (kwefayile ezinebug!)"

#: rc.cpp:16
msgid "Do &not use the hint! (Recommanded!)"
msgstr "Su&ku sebenzisa ukuthi tshuphe! (Iyalelwe kwakhona!)"
