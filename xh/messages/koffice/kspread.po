msgstr "ELINAMAQELA(1.2;3.4) ibuyisela \"1.2+3.4i\""

#: xml_doc.cc:495
msgid "COMPLEX(0;-1) returns \"-i\""
msgstr "ELINAMAQELA(0;-1) ibuyisela\"-i\""

