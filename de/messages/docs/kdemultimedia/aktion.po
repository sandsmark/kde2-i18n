#: index.docbook:56
msgid "&aktion; is a video player"
msgstr "&aktion; ist ein Programm zur Wiedergabe von Videos in KDE."

#: index.docbook:60
msgid "<keyword>KDE</keyword>"
msgstr "<keyword>KDE</keyword>"

#: index.docbook:61
msgid "aKtion"
msgstr "aKtion"

#: index.docbook:62
msgid "kdemultimedia"
msgstr "kdemultimedia"

#: index.docbook:63
msgid "mpeg"
msgstr "mpeg"

#: index.docbook:64
msgid "<keyword>avi</keyword>"
msgstr "<keyword>avi</keyword>"

#: index.docbook:65
msgid "QuickTime"
msgstr "QuickTime"

#: index.docbook:66
