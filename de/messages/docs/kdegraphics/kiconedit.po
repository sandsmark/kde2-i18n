msgid "<guimenu><accel>T</accel>ools</guimenu> <guimenuitem>Spray</guimenuitem>"
msgstr "<guimenu><accel>W</accel>erkzeuge</guimenu> <guimenuitem>Spray</guimenuitem>"

#: index.docbook:703
msgid "Draws a random dotted pattern like a spraycan."
msgstr "Zeichnet verstreute Pixel, ähnlich einer Sprühdose"

#: index.docbook:703
