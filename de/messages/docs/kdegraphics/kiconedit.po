msgid ""
"<guimenu><accel>V</accel>iew</guimenu> <guimenuitem>Zoom "
"<accel>o</accel>ut</guimenuitem>"
msgstr ""
"<guimenu><accel>A</accel>nsicht</guimenu> "
"<guimenuitem>Ver<accel>k</accel>leinern</guimenuitem>"

#: index.docbook:656
msgid "Shrink the view icon to a smaller screen size"
msgstr "Verkleinert die Symbolansicht"

#: index.docbook:656
msgid "<guimenu><accel>V</accel>iew</guimenu> <guisubmenu>Zoom factor</guisubmenu>"
msgstr ""
"<guimenu><accel>A</accel>nsicht</guimenu> "
"<guisubmenu>Vergrößerungsfaktor</guisubmenu>"

#: index.docbook:656
msgid "Zoom to a predefined zoom factor"
msgstr "Auf einen vordefinierten Faktor vergrößern"

