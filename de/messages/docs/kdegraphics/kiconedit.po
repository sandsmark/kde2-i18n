#: index.docbook:703
msgid "<guimenu><accel>T</accel>ools</guimenu> <guimenuitem>Circle</guimenuitem>"
msgstr "<guimenu><accel>W</accel>erkzeuge</guimenu> <guimenuitem>Kreis</guimenuitem>"

#: index.docbook:703
msgid ""
"<guimenu><accel>T</accel>ools</guimenu> <guimenuitem>Filled "
"circle</guimenuitem>"
msgstr ""
"<guimenu><accel>W</accel>erkzeuge</guimenu> <guimenuitem>Gefüllter "
"Kreis</guimenuitem>"

#: index.docbook:703
msgid "<guimenu><accel>T</accel>ools</guimenu> <guimenuitem>Ellipse</guimenuitem>"
msgstr ""
"<guimenu><accel>W</accel>erkzeuge</guimenu> "
"<guimenuitem>Ellipse</guimenuitem>"

#: index.docbook:703
msgid ""
"<guimenu><accel>T</accel>ools</guimenu> <guimenuitem>Filled "
"ellipse</guimenuitem>"
