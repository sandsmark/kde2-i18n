msgid ""
"<command>khelpcenter</command> <option>file:/ "
"<replaceable>usr/local/src/qt/html/index.html</replaceable></option>"
msgstr ""
"khelpcenter "
"<option><replaceable>file:/usr/local/src/qt/html/index.html</replaceable>"
"</option>"

#: index.docbook:57
msgid "A Man page"
msgstr "Eine Handbuch-Seite (man page)"

#: index.docbook:57
#, fuzzy
msgid ""
"<command>khelpcenter</command> "
"<option>man:/<replaceable>strcpy</replaceable></option>"
msgstr "khelpcenter <option>man:/<replaceable>strcpy</replaceable></option>"

#: index.docbook:57
msgid "An Info page"
msgstr "Eine Info-Seite"

#: index.docbook:57
#, fuzzy
msgid ""
"<command>khelpcenter</command> "
"<option>info:/<replaceable>gcc</replaceable></option>"
