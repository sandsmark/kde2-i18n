msgstr "Verschiedene Farbschema werden mit dem &kde; geliefert. Diese beinhalten:"

#: index.docbook:100
msgid "&kde; default,"
msgstr "&kde; Standard,"

#: index.docbook:100
msgid "Atlas green"
msgstr "Atlasgrün"

#: index.docbook:100
msgid "BeOS,"
msgstr "BeOS,"

#: index.docbook:100
msgid "Blue slate,"
msgstr "Blauer Schiefer"

#: index.docbook:100
msgid "<member>CDE</member>"
msgstr "<member> CDE</member>"

#: index.docbook:100
msgid "Dark Blue,"
msgstr "Dunkelblau"

#: index.docbook:100
msgid "Desert red,"
msgstr "Wüstenrot"

#: index.docbook:100
msgid "Digital CDE,"
msgstr "Digitales CDE"

#: index.docbook:100
msgid "Pale gray,"
msgstr "Bleiches Grau"

#: index.docbook:100
msgid "Pumpkin,"
msgstr "Kürbis"

#: index.docbook:100
msgid "Solaris CDE,"
msgstr "Solaris"

#: index.docbook:100
msgid "Storm,"
msgstr "Sturm"

#: index.docbook:100
msgid "Redmond,"
msgstr "Windows 2000"

#: index.docbook:100
msgid "System,"
msgstr "System"

#: index.docbook:100
msgid "Next,"
msgstr "Next,"

#: index.docbook:100
msgid "Point Reyes Green,"
msgstr "Point Reyes-Grün,"

#: index.docbook:100
msgid "Media Peach"
msgstr "Media Peach"

#: index.docbook:100
