#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Felhasználónév (akinek a jelszavát meg kell változtatni)."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "A Unix jelszó megváltoztatása."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Karbantartó"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "A jelszó megváltozott."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "A jelszó megváltoztatása"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Adja meg a mostani jelszavát."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "A kommunikáció megszakadt a 'passwd' programmal."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "A 'passwd' program nem található."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "A megadott jelszó hibás! Írja be még egyszer."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Belső hiba: érvénytelen visszaadott érték a PasswdProcess::checkCurrent "
"függvényhívásnál."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Adja meg az új jelszót."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "A jelszónak csak az első 8 karakterét tárolja el a rendszer."

