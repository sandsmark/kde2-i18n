#: plugins/pgioptions/pgioptionsplugin.cpp:124
msgid "No Optimization"
msgstr "nincs optimalizálás"

#: plugins/gccoptions/gccoptionsplugin.cpp:172
#: plugins/pgioptions/pgioptionsplugin.cpp:125
msgid "Level 1"
msgstr "1. szint"

#: plugins/gccoptions/gccoptionsplugin.cpp:173
#: plugins/pgioptions/pgioptionsplugin.cpp:126
msgid "Level 2"
msgstr "2. szint"

#: plugins/gccoptions/gccoptionsplugin.cpp:178
msgid "<qt>Do not store floating point variables in registers.</qt>"
msgstr ""
"<qt>A lebegőpontos változók regiszterben való tárolásának "
"letiltása.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:181
msgid ""
"<qt>Pop the arguments to each function call directly after the function "
"returns.</qt>"
msgstr ""
"<qt>A függvényargumentumok eltávolítása a veremből közvetlenül a "
"függvényhívás után.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:185
msgid ""
"<qt>Force memory operands to be copied into registers before doing "
"arithmetic on them.</qt>"
msgstr ""
"<qt>A memóriában tárolt változók regiszterbe másolása "
"műveletvégzés előtt.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:189
msgid ""
"<qt>Force memory address constants to be copied into registers before doing "
"arithmetic on them.</qt>"
msgstr ""
"<qt>A memóriacím-állandók regiszterbe másolása műveletvégzés "
"előtt.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:193
msgid ""
"<qt>"
"Don't keep the frame pointer in a register for functions that don't need "
"one.</qt>"
msgstr ""
"<qt>A veremmutató regiszterben tárolása olyan függvényeknél is, "
"amelyek azt nem használják.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:197
msgid "<qt>Ignore the <i>inline</i> keyword.</qt>"
msgstr "<qt>Az <i>inline</i> kulcsszó figyelmen kívül hagyása.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:202
msgid ""
"<qt>Do not make members functions inline merely because they are defined "
"inside the class scope.</qt>"
msgstr ""
"<qt>Ne legyenek a tagfüggvények 'inline' típusúak csak azért, mert az "
"osztály deklarációján belül lettek megvalósítva.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:259
msgid "Dialect"
msgstr "Nyelvi változatok"

#: plugins/gccoptions/gccoptionsplugin.cpp:261
msgid "Interpret source code as Fortran 90 free form."
msgstr "A forráskód kötetlen formájú Fortran 90-ként való kezelése."

#: plugins/gccoptions/gccoptionsplugin.cpp:264
msgid "Allow certain Fortran 90 constructs."
msgstr "Bizonyos Fortran 90 konstrukciók engedélyezése."

#: plugins/gccoptions/gccoptionsplugin.cpp:266
msgid "Allow '$' in symbol names."
msgstr "A '$' karakter engedélyezése szimbólumnevekben."

#: plugins/gccoptions/gccoptionsplugin.cpp:268
msgid "Allow '' in character constants to escape special characters."
msgstr ""
"A '' engedélyezése karakterállandókban speciális karakterek "
"definiálásához."

#: plugins/gccoptions/gccoptionsplugin.cpp:271
msgid "DO loops are executed at least once."
msgstr "A DO ciklusok végrehajtása legalább egyszer megtörténik."

#: plugins/gccoptions/gccoptionsplugin.cpp:278
msgid "Treat local variables as if SAVE statement had been specified."
msgstr ""
"A helyi változókat úgy kell kezelni, mintha egy SAVE utasítás lett "
"volna kiadva."

#: plugins/gccoptions/gccoptionsplugin.cpp:280
msgid "Init local variables to zero."
msgstr "A helyi változók 0-ra inicializálása."

#: plugins/gccoptions/gccoptionsplugin.cpp:282
msgid "Generate run-time checks for array subscripts."
msgstr "A tömbindexek ellenőrzése futás közben."

#: plugins/gccoptions/gccoptionsplugin.cpp:314
msgid "Inhibit all warnings."
msgstr "Az összes figyelmeztetés letiltása."

#: plugins/gccoptions/gccoptionsplugin.cpp:316
msgid "Inhibit warnings about the use of #import."
msgstr "Minden, az '#import' használatával kapcsolatos figyelmeztetés letiltása."

#: plugins/gccoptions/gccoptionsplugin.cpp:318
msgid "Make all warnings into errors."
msgstr "A figyelmeztetések hibaként kezelése."

#: plugins/gccoptions/gccoptionsplugin.cpp:320
msgid "Issue all warnings demanded by strict ANSI C or ISO C++."
msgstr ""
"A 'strict' ANSI C és ISO C++ által megkövetelt összes figyelmeztetés "
"generálása."

#: plugins/gccoptions/gccoptionsplugin.cpp:322
msgid "Like -pedantic, but errors are produced instead of warnings."
msgstr "Hasonló a -pedantic-hoz, de a figyelmeztetések hibának számítanak."

#: plugins/gccoptions/gccoptionsplugin.cpp:324
msgid "All below warnings combined:"
msgstr "Az összes alábbi figyelmeztetés:"

#: plugins/gccoptions/gccoptionsplugin.cpp:329
msgid "<qt>Warn if an array subscript has type <i>char</i>.</qt>"
msgstr "<qt>Figyelmeztetés, ha egy tömbindex <i>char</i> típusú.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:331
msgid "<qt>Warn when a comment-start sequence /* appears inside a comment.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy megjegyzéskezdő /* szimbólum megjegyzésen "
"belül szerepel.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:333
msgid "<qt>Check calls to <i>printf()</i>, <i>scanf()</i> etc.</qt>"
msgstr ""
"<qt>A <i>printf()</i>, <i>scanf()</i> stb. függvényhívások "
"ellenőrzése.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:335
msgid "<qt>Warn when a declaration does not specify a type.</qt>"
msgstr "<qt>Figyelmeztetés, ha egy deklarációnál nincs megadva a típus.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:338
msgid "<qt>Issue a warning when a non-declared function is used.</qt>"
msgstr ""
"<qt>Figyelmeztetés deklaráció nélküli függvény felhasználása "
"esetén.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:341
msgid "<qt>Issue an error when a non-declared function is used.</qt>"
msgstr ""
"<qt>Hibaüzenet deklaráció nélküli függvény felhasználása "
"esetén.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:343
msgid "<qt>Warn if the type of <i>main()</i> is suspicious.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha a <i>main()</i> típusa feltehetően nem "
"megfelelő.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:345
msgid "<qt>Warn when multicharacter constants are encountered.</qt>"
msgstr "<qt>Figyelmeztetés többkarakteres állandók használata esetén.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:347
msgid "<qt>Warn when parentheses are omitted in certain context.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha bizonyos konstrukciókban elmaradnak a "
"zárójelek.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:349
msgid "<qt>Warn when a function without explicit return type is defined.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy függvénynél nincs megadva a visszaadott "
"érték típusa.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:351
msgid "<qt>Warn when trigraphs are encountered.</qt>"
msgstr "<qt>Figyelmeztetés trigraph-ok használata esetén.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:353
msgid "<qt>Warn when a variable is declared but not used.</qt>"
msgstr "<qt>Figyelmeztetés a deklarált, de fel nem használt változókra.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:355
msgid "<qt>Warn when a variable is used without being initialized before.</qt>"
msgstr "<qt>Figyelmeztetés nem inicializált változó használata esetén.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:357
msgid "<qt>Warn when an unknown #pragma statement is encountered.</qt>"
msgstr "<qt>Figyelmeztetés ismeretlen #pragma utasítás használata esetén.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:360
msgid ""
"<qt>Warn when the order of member initializers is different from\n"
"the order in the class declaration.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha a tagok inicializálási sorrendje eltér az "
"osztálydeklarációban\n"
"megadott sorrendtől.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:395
msgid "<qt>Set options not included in -Wall which are very specific.</qt>"
msgstr "<qt>A -Wall opcióban nem szereplő figyelmeztetések bekapcsolása.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:397
msgid ""
"<qt>Warn about certain constructs that behave differently\n"
"in traditional and ANSI C.</qt>"
msgstr ""
"<qt>Figyelmeztetés az olyan szerkezeteknél, amelyek eltérően\n"
"viselkednek a hagyományos és az ANSI C esetében.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:400
msgid "<qt>Warn if an undefined identifier is evaluated in an `#if' directive</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha nem definiált azonosító található egy `#if' "
"utasításban</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:402
msgid "<qt>Warn whenever a local variable shadows another local variable.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy lokális változó takar egy másik lokális "
"változót.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:404
msgid ""
"<qt>Warn about anything that depends on the <i>sizeof</i> a\n"
"function type or of <i>void</i>.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha valami egy függvénytípus vagy a <i>void</i>\n"
"<i>sizeof</i> értékétől függ.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:407
msgid "<qt>Warn whenever a function call is cast to a non-matching type.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy függvény típusa egy vele nem egyeztethető "
"típusra van átalakítva.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:409
msgid ""
"<qt>Warn whenever a pointer is cast so as to remove a type\n"
"qualifier from the target type.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha olyan mutatókonverzió történik, amikor az "
"eredeti típus minősítése törlődik.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:412
msgid ""
"<qt>Warn whenever a pointer is cast such that the required\n"
"alignment of the target is increased.</qt>"
msgstr ""
"<qt>Figyelmezetés, ha egy mutató olyan típusátalakítást kapott, "
"amelynek\n"
"hatására a cél igazítása megnő.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:415
msgid ""
"<qt>Warn when the address of a string constant is cast\n"
"into a non-const <i>char *</i> pointer</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy sztringkonstans címe\n"
"nem const típusú <i>char *</i> mutatóvá van átalakítva.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:418
msgid ""
"<qt>Warn if a prototype causes a type conversion that is different\n"
"from what would happen to the same argument in the absence\n"
"of a prototype.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy prototípus olyan típusátalakítást "
"eredményez,\n"
"amely eltér attól, ami a prototípus nélküli\n"
"átalakítás során jönne létre.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:422
msgid ""
"<qt>Warn when a comparison between signed and unsigned values\n"
"could produce an incorrect result when the signed value\n"
"is converted to unsigned.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy előjeles és egy előjel nélküli érték\n"
"összehasonlítása hibás eredményt okozna az előjeles érték\n"
"előjel nélkülivé konvertálása esetén."

#: plugins/gccoptions/gccoptionsplugin.cpp:426
msgid ""
"<qt>Warn if any functions that return structures or unions are\n"
"defined or called.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha a programban olyan függvényt deklarál vagy "
"használ, amely struktúrát vagy uniót ad vissza.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:429
msgid ""
"<qt>Warn if a function is declared or defined without specifying\n"
"the argument types.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy függvény deklarációja vagy megvalósítása "
"nem tartalmazza\n"
"az argumentumok típusát.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:432
msgid ""
"<qt>Warn if a global function is defined without a previous prototype "
"declaration.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy globális függvény megvalósítását nem "
"előzi meg a prototípus deklarációja.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:434
msgid "<qt>Warn if a global function is defined without a previous declaration.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy globális függvény megvalósítását nem "
"előzi meg deklaráció.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:436
msgid "<qt>Warn if anything is declared more than once in the same scope</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha valami egynél többször van deklarálva az adott "
"programkörnyezeten belül.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:438
msgid ""
"<qt>Warn if an <i>extern</i> declaration is encountered within a "
"function.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha <i>extern</i> deklaráció található egy "
"függvényen belül.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:440
msgid "<qt>Warn if an <i>inline</qt> function can not be inlined</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy <i>inline</qt> függvény nem kezelhető inline "
"módon</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:442
msgid "<qt>Warn if an old-style (C-style) cast is used within a program</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha régi formátumú (C-stílusú) átalakítást "
"használ a program.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:444
msgid "<qt>Warn if the <i>long long</i> type is used.</qt>"
msgstr "<qt>Figyelmeztetés a <i>long long</i> típus használatakor.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:448
msgid ""
"<qt>Warn when a derived class function declaration may be an\n"
"error in defining a virtual function.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha egy tagfüggvény deklarációja hibás virtuális\n"
"függvénymegvalósítást eredményezne.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:451
msgid ""
"<qt>Warn when g++'s synthesis behavior does\n"
"not match that of cfront.</qt>"
msgstr ""
"<qt>Figyelmeztetés, ha a g++ szintéziskezelése\n"
"eltér a cfront-étól.</qt>"

