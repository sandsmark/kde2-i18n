msgstr "Egyéb parancssori opciók"

#: index.docbook:133
msgid "The following command line help options are available"
msgstr ""
"A következő parancssori opciók információt adnak a programmal "
"kapcsolatban:"

#: index.docbook:135
msgid "<command>kwrite</command> <option>--help</option>"
msgstr "<command>kwrite</command> <option>--help</option>"

#: index.docbook:135
msgid "This lists the most basic options available at the command line."
msgstr "A legalapvetőbb parancssori opciók listázása."

#: index.docbook:135
msgid "<command>kwrite</command> <option>--help-qt</option>"
msgstr "<command>kwrite</command> <option>--help-qt</option>"

#: index.docbook:135
msgid ""
"This lists the options available for changing the way &kwrite; interacts "
"with &Qt;."
msgstr "A &kwrite; és a &Qt; közti kapcsolatot szabályzó opciók listázása."

#: index.docbook:135
msgid "<command>kwrite</command> <option>--help-kde</option>"
msgstr "<command>kwrite</command> <option>--help-kde</option>"

#: index.docbook:135
msgid ""
"This lists the options available for changing the way &kwrite; interacts "
"with KDE."
msgstr "A &kwrite; és a KDE közti kapcsolatot szabályzó opciók listázása."

#: index.docbook:135
msgid "<command>kwrite</command> <option>--help-all</option>"
msgstr "<command>kwrite</command> <option>--help-all</option>"

#: index.docbook:135
msgid "This lists all of the command line options."
msgstr "Az összes parancssori opció listázása."

#: index.docbook:135
msgid "<command>kwrite</command> <option>--author</option>"
msgstr "<command>kwrite</command> <option>--author</option>"

#: index.docbook:135
msgid "Lists &kwrite;'s authors in the terminal window"
msgstr "A &kwrite; szerzőinek listázása."

#: index.docbook:135
msgid "<command>kwrite</command> <option>--version</option>"
msgstr "<command>kwrite</command> <option>--version</option>"

#: index.docbook:135
msgid ""
"Lists version information for &Qt;, KDE, and &kwrite;. Also available "
