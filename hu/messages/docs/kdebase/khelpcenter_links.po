msgstr "developer.kde.org"

#: index.docbook:14
msgid "The &kde; developers centre"
msgstr "A &kde;-fejlesztők központi site-ja"

#: index.docbook:14
msgid "webcvs.kde.org"
msgstr "webcvs.kde.org"

#: index.docbook:14
msgid "A web interface to the &kde; cvs repository"
msgstr "Böngészőből kezelhető felület a &kde; CVS szoftverraktárhoz"

