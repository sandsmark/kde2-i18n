msgid ""
"<shortcut> <keycap>P</keycap> </shortcut> "
"<guimenu><accel>G</accel>ame</guimenu> "
"<guimenuitem><accel>P</accel>ause/Continue</guimenuitem>"
msgstr ""
"<shortcut> <keycap>P</keycap> </shortcut> "
"<guimenu><accel>J</accel>áték</guimenu> "
"<guimenuitem><accel>S</accel>zünet/folytatás</guimenuitem>"

