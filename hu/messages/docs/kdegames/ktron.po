msgid ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>Q</keycap></keycombo> "
"</shortcut> <guimenu><accel>G</accel>ame</guimenu> "
"<guimenuitem><accel>E</accel>xit</guimenuitem>"
msgstr ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>Q</keycap></keycombo> "
"</shortcut> <guimenu><accel>J</accel>áték</guimenu> "
"<guimenuitem>S<accel>z</accel>erkesztés</guimenuitem>"

