msgid "pow(value;value)"
msgstr "pow(érték;érték)"

#: xml_doc.cc:318
msgid "pow(1.2;3.4) equals 1.8572"
msgstr "pow(1,2;3,4) értéke 1,8572"

#: xml_doc.cc:319
msgid "pow(2;3) equals 8"
msgstr "pow(2;3) értéke 8"

#: xml_doc.cc:322
