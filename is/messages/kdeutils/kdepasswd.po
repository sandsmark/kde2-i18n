#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Breyta lykilorði þessa notanda."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE lykilorð"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Breytir Unix lykilorði."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Umsjónarmaður"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Lykilorðinu þínu hefur verið breytt."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Breyta lykilorði"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Sláðu inn núverandi lykilorð"

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Samskipti við `passwd' brugðust."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Gat ekki fundið `passwd' forritið."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Rangt lykilorð! Reyndu aftur."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Innvær villa: ólöglegt skilagildi úr PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Sláðu inn nýja lykilorðið."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Lykilorðið þitt verður takmarkað við 8 stafi."

