msgid "<h1>List of collected notes</h1>"
msgstr "<h1>Listi uppsafnaðra skilaboða</h1>"

#: kscoring.cpp:343
msgid "Collected Notes"
msgstr "Uppsöfnuð skilaboð"

#: kscoring.cpp:402
msgid "contains substring"
msgstr "inniheldur hlutstrenginn"

#: kscoring.cpp:403
msgid "matches regular expression"
msgstr "fellur að reglulegu segðinni"

#: kscoring.cpp:404
msgid "is exactly the same as"
msgstr "er nákvæmlega eins og"

#: kscoring.cpp:405
msgid "less than"
msgstr "minna en"

#: kscoring.cpp:406
msgid "greater than"
msgstr "meira en"

#: kscoring.cpp:929
msgid "the rule name is already assigned, please choose another name"
msgstr "búið er að nota þetta nafn, veldu annað"

#: kscoring.cpp:1056
#, c-format
msgid "rule %1"
msgstr "regla %1"

#: kscoringeditor.cpp:69
msgid "Not"
msgstr "Ekki"

#: kscoringeditor.cpp:70
msgid "negate this condition"
msgstr "nota andhverfa skilyrðið"

#: kscoringeditor.cpp:74
msgid "select the header against this condition matches"
msgstr "veldu það svið í hausnum skilyrðið á við"

#: kscoringeditor.cpp:78
msgid "select the type of match"
msgstr "veldu samanburðaraðferð"

#: kscoringeditor.cpp:81
msgid "the condition for the match"
msgstr "skilyrði samanburðarins"

