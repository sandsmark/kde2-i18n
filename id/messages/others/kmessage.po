#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"penggunaan: %s [-pilihan] [pesan ....]\n"
"\n"

#: options.cpp:225
msgid "where options include:\n"
msgstr "Dimana pilihan termasuk:\n"

#: options.cpp:226
msgid "  -title name             the title used on the message window\n"
msgstr "Nama judul.........judul digunakan di pesan window\n"

#: options.cpp:227
msgid "  -file filename          file to read message from, \"-\" for stdin\n"
msgstr "berkas nama berkas.......berkas untuk dibaca pesan dari, \"-\"for stdin\n"

#: options.cpp:228
msgid "  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr "Sting tombol ......perintah-daftar batasan tabel[kode keluar]\n"

#: options.cpp:229
msgid "  -default button         button to activate if Return is pressed\n"
msgstr "tombol standar......tombol untuk mengaktifkan jika dilanjutkan telah ditekan\n"

#: options.cpp:230
msgid "  -cancel button          button to activate if Escape is pressed\n"
msgstr "tombol pembatalan.........tombol untuk mengaktifkan jika kabur telah ditekan\n"

#: options.cpp:231
msgid "  -print                  print the button label when selected\n"
msgstr "Cetak                  Cetak label tombol ketika dipilih\n"

#: options.cpp:232
msgid "  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr "kabur           expand \\n,\\r, and \\t karakter di pesan\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"detik timeout        keluar dengan status 0 setelah \"det\"detik\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr "pilihan diijinkam untuk kompatibilitas, tetapi tidak berfungsi:\n"

#: options.cpp:236
msgid "  -center\n"
msgstr "Pusat\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"Dekat mouse\n"
"\n"

