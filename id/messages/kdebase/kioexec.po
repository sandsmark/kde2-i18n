#: main.cpp:45
msgid "KFM Exec - Opens remote files, watches modifications, asks for upload"
msgstr "KFM Exec - Membuka file remote, melihat modifikasi, bertanya untuk upload"

#: main.cpp:51
msgid "Command to execute."
msgstr "Perintah untuk dijalankan."

#: main.cpp:52
msgid "URL(s) or local file used for 'command'."
msgstr "URL atau berkas lokal yang digunakan untuk 'perintah'."

#: main.cpp:68
msgid ""
"'command' expected.\n"
msgstr ""
"Diharapkan 'perintah'.\n"

#: main.cpp:89
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"URL %1\n"
"salah bentuk"

#: main.cpp:196
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"Berkas\n"
"%1\n"
"telah dimodifikasi.\n"
"Anda ingin mengupload perubahan?"

#: main.cpp:197
msgid "File changed"
msgstr "Berkas berubah"

#: main.cpp:256
msgid "KFMExec"
msgstr "KFMExec"

#: main.cpp:271
msgid ""
"Syntax Error:\n"
"kfmexec command [URLs ....]"
msgstr ""
"Salah Sintaks:\n"
"kfmexec perintah [URL...]"

