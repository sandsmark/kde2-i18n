#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr "Tool KDE untuk membuka URL dari command line"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Tunjukkan perintah yang tersedia."

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Perintah (lihat --perintah)."

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Argumen untuk perintah."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Sintaks:\n"

#: kfmclient.cc:77
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmcilent openURL 'url' ['tipemime']\n"
"     # Membuka window tampilkan 'url'.\n"
"     #  'url' bisa relatif path\n"
"     #  atau nama file, seperti . atau subdir/\n"
"     # Jika 'url' diabaikan, $HOME yang digunakan.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # Jika 'tipemime' disebutkan, akan digunakan untuk mementukan\n"
"            #   komponen yang perlu digunakan konqueror. Sebagai contoh, "
"tentukan ke\n"
"            #   text/html untuk halaman web, agar munculnya lebih cepat\n"
"\n"

#: kfmclient.cc:86
#, fuzzy
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profile'\n"
"     # Membuka window menggunakan profile yang diberikan.\n"
"     #   'profile' adalah file di bawah "
"~/.kde/share/apps/konqueror/profiles.\n"
"     #   'url' adalah URL optional untuk dibuka.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"     # Membuka menu properties\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['binding']]\n"
"     # Mencoba untuk mengeksekusi 'url'. 'url' bisa seperti biasa\n"
"     #   URL, URL ini akan dibuka. Anda bisa mengabaikan\n"
"     #   'binding', Pada kasus ini binding default\n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"     #   dicoba. Tentu saja URL bisa URL dari sebuah\n"
"     #   dokumen, atau sebuah file *.desktop.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"     #   Dengan cara ini misalnya Anda bisa me-mount divais\n"
"     #   dengan melewati 'Mount default' sebagai binding ke \n"
"     #   'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"kfmclient move 'src' 'dest'\n"
"     # Memindahkan URL 'src' ke 'dest'.\n"
"     #   'src' bisa sebagai daftar beberapa URL.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"kfmclient copy 'src' 'dest'\n"
"     # Mengkopi URL 'src' ke 'dest'.\n"
"     #   'src' bisa sebagai daftar URL-URL.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"     #  Menata ulang semua ikon pada desktop.\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"     # Membaca ulang konfigurasi konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"     # Membaca ulang konfigurasi kdesktop.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Contoh:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"     // Mount CDROM\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Buka berkas dengan default binding\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Buka berkas dengan netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Buka jendela baru dengan URL\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Memulai emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Membuka direktori mount CD-ROM\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // Membuka current directory.\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"Profil %1 tidak ditemukan\n"

#: kfmclient.cc:290
msgid ""
"Syntax Error: Not enough arguments\n"
msgstr ""
"Salah sintaks: Argumen kurang\n"

#: kfmclient.cc:295
msgid ""
"Syntax Error: Too many arguments\n"
msgstr ""
"Salah sintaks: argumen terlalu banyak\n"

#: kfmclient.cc:417
msgid ""
"Syntax Error: Unknown command '%1'\n"
msgstr ""
"Salah sintaks: Perintah '%1' tidak dikenal\n"

