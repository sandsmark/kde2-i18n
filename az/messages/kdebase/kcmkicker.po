msgid "&Position"
msgstr "&Yer"

#: main.cpp:62
msgid "&Hiding"
msgstr "&Gizləmə"

#: main.cpp:66
msgid "&Look && Feel"
msgstr "&Görünüş  && Toxuma"

#: main.cpp:70
msgid "&Menus"
msgstr "&Menyular"

