#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Bu istifadəçinin parolunu dəyişdir."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE parolu"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Bir UNIX parolunu dəyişdirər."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "İdarəçi"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Parolunuz dəyişdirildi."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Parol dəyişdir"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Lütfən, indiki parolunuzu giriniz."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "'passwd' ilə danışıq apaıla bilmədi"

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "'passwd' proqramı tapıla bilmədi"

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Səhv parol! Lütfən, bir də sınayın."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Daxili xəta: PasswdProcess'dən qanuni olmayan dönüş qiyməti: "
"checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Lütfən, yeni parolunuzu giriniz."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Parolunuz ən az 8 hərf olmalıdır"

