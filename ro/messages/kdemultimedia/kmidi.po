#: kmidiframe.cpp:249
msgid ""
"The choruslevel<br>\n"
"is set to a minimum level of 32."
msgstr "Nivelul de ecou este setat la minim 32."

#: kmidiframe.cpp:252
msgid ""
"The choruslevel<br>\n"
"is set to a minimum level of 64."
msgstr "Nivelul de ecou este setat la minim 64."

#: kmidiframe.cpp:255
msgid ""
"The choruslevel<br>\n"
"is set to a minimum level of 96."
msgstr "Nivelul de ecou este setat la minim 96."

#: kmidiframe.cpp:258
msgid ""
"The choruslevel<br>\n"
"is set to the maximum level of 127."
msgstr "Nivelul de ecou este setat la maxim 127."

#: kmidiframe.cpp:263
msgid "No detune"
msgstr "Fără dezacord"

