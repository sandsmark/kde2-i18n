#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"folosire: %s [-optiuni] [mesaj ...]\n"
"\n"

#: options.cpp:225
msgid ""
"where options include:\n"
msgstr ""
"unde optiunile sînt:\n"

#: options.cpp:226
msgid ""
"  -title name             the title used on the message window\n"
msgstr ""
"  -title nume             titlul utilizat în fereastra de mesaj\n"

#: options.cpp:227
msgid ""
"  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -file numefisier          fisierul din care se citeste mesajul, \"-\" "
"pentru STDIN\n"

#: options.cpp:228
msgid ""
"  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
"  -buttons sirtext         lista de eticheta[:cod_iesire] separate prin "
"virgule\n"

#: options.cpp:229
msgid ""
"  -default button         button to activate if Return is pressed\n"
msgstr ""
"  -default buton          butonul activat daca se apasa Return (sau Enter)\n"

#: options.cpp:230
msgid ""
"  -cancel button          button to activate if Escape is pressed\n"
msgstr ""
"  -cancel buton           butonul activat daca se apasa Escape\n"

#: options.cpp:231
msgid ""
"  -print                  print the button label when selected\n"
msgstr ""
"  -print                  scrie eticheta butonul cind este selectat\n"

#: options.cpp:232
msgid ""
"  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -escape                 expandează caracterele \\n,\\r si \\t din mesaj\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -timeout sec            iese cu codul de stare 0 dupa \"sec\" secunde\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"urmatoarele optiuni sint permise pentru compatibilitate, dar nu sint "
"functionale:\n"

#: options.cpp:236
msgid ""
"  -center\n"
msgstr ""
"  -center\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -nearmouse\n"
"\n"

#: options.cpp:239
#, c-format
msgid ""
"%s version %s\n"
msgstr ""
"%s versiunea %s\n"

#: options.cpp:240
#, c-format
msgid ""
"by %s\n"
msgstr ""
"de %s\n"

