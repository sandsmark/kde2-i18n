#: main.cpp:45
msgid "KFM Exec - Opens remote files, watches modifications, asks for upload"
msgstr ""
"KFM Exec - Deschide fisiere distante, urmareste modificarile, confirma "
"actiunile de transfer (upload)"

#: main.cpp:51
msgid "Command to execute."
msgstr "Comanda de executat."

#: main.cpp:52
msgid "URL(s) or local file used for 'command'."
msgstr ""
"URL-ul(uri) sau fisierul local utilizat\n"
"pentru 'comanda'."

#: main.cpp:68
msgid ""
"'command' expected.\n"
msgstr ""
"astept o 'comandă' de executat.\n"

#: main.cpp:89
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"URL-ul %1\n"
"este eronat."

#: main.cpp:196
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"Fişierul\n"
"%1\n"
"a fost modificat.\n"
"Doriţi să transfer modificările?"

#: main.cpp:197
msgid "File changed"
msgstr "Fişier modificat"

#: main.cpp:256
msgid "KFMExec"
msgstr "KFMExec"

#: main.cpp:271
msgid ""
"Syntax Error:\n"
"kfmexec command [URLs ....]"
msgstr ""
"Eroare de sintaxa:\n"
"kfmexec comanda [URL-uri ....]"

