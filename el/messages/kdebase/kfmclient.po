#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr ""
"Εργαλείο του KDE για το άνοιγμα URL από τη "
"γραμμή εντολών"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Προβολή διαθέσιμων εντολών."

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Εντολή (βλ. --commands)"

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Ορίσματα για την εντολή."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Σύνταξη:\n"

#: kfmclient.cc:77
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Ανοίγει ένα παράθυρο που δείχνει "
"το 'url'.\n"
"            #  το 'url' μπορεί να είναι μια σχετική "
"διαδρομή\n"
"            #   ή όνομα αρχείου, όπως . ή subdir/\n"
"            #   Αν το 'url' παραλειφθεί, "
"χρησιμοποιείται το $HOME.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # Αν καθοριστεί το 'mimetype' θα "
"χρησιμοποιηθεί για να καθοριστεί\n"
"            #   το τμήμα του konqueror που θα "
"χρησιμοποιηθεί. Για παράδειγμα δώστε\n"
"            #   text/html για μια ιστοσελίδα, ώστε να "
"εμφανιστεί ταχύτερα\n"
"\n"

#: kfmclient.cc:86
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'προφίλ' ['url']\n"
"            # Ανοίγει ένα παράθυρο με το δοθέν "
"προφίλ.\n"
"            #  Το 'προφίλ' είναι ένα αρχείο στο "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #  Το 'url' είναι ένα προαιρετικό URL "
"για άνοιγμα.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"            # Ανοίγει ένα μενού Ιδιοτήτων\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Προσπαθεί να εκτελέσει το 'url'. Το "
"'url' μπορεί να είναι\n"
"            # ένα συνηθισμένο  URL, οπότε θα "
"ανοιχτεί. Μπορείτε να\n"
"            # παραλείψετε το 'binding'. Σ' αυτήν την "
"περίπτωση το προκαθορισμένο binding\n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   δοκιμάζεται. Φυσικά το URL μπορεί "
"να είναι URL ενός\n"
"            #   εγγράφου, ή ένα αρχείο *.desktop.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   Έτσι μπορείτε π.χ. να προσδέσετε "
"μια συσκευή, \n"
"            #   περνώντας το 'Mount default' σαν binding "
"στο \n"
"            #   'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient move 'src' 'dest'\n"
"            # Μετακινεί το URL 'src' στο 'dest'.\n"
"            #   'src' μπορεί να είναι μια λίστα με "
"URL.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient copy 'src' 'dest'\n"
"            # Αντιγράφει το URL 'src' στο 'dest'.\n"
"            # το 'src' μπορεί να είναι μια λίστα με "
"URL.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"            # Τακτοποιεί όλα τα εικονίδια στην "
"επιφ. εργασίας.\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"            # Ξαναδιαβάζει τις ρυθμίσεις του "
"konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"            # Ξαναδιαβάζει τις ρυθμίσεις του "
"kdesktop.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Παραδείγματα:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // πρόσδεση του CDROM\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Ανοίγει το αρχείο με το "
"προκαθορισμένο binding\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Ανοίγει το αρχείο με το netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Ανοίγει νέο παράθυρο με το URL\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Εκίνηση του emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Ανοίγει τον κατάλογο πρόσδεσης "
"του CDROM\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // Ανοίγει τον τρέχοντα κατάλογο. "
"Πολύ βολικό.\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"Το προφίλ %1 δε βρέθηκε\n"

#: kfmclient.cc:290
msgid ""
"Syntax Error: Not enough arguments\n"
msgstr ""
"Συντακτικό Σφάλμα: Όχι αρκετά ορίσματα\n"

#: kfmclient.cc:295
msgid ""
"Syntax Error: Too many arguments\n"
msgstr ""
"Συντακτικό Σφάλμα: Πάρα πολλά ορίσματα\n"

#: kfmclient.cc:417
msgid ""
"Syntax Error: Unknown command '%1'\n"
msgstr ""
"Συντακτικό Σφάλμα: Άγνωστη εντολή '%1'\n"

