msgstr "<b>Картинки</b><p></b>"

#: rc.cpp:32
msgid "&Suppress images"
msgstr "&Забранява картинки"

#: rc.cpp:33
msgid ""
"<b>Suppress images</b><p>Selecting this will prevent Konqueror from loading "
"images.</p>"
msgstr ""
"<b>Забранява картинки</b><p> Konqueror няма да "
"показва картинките.</p>"

#: rc.cpp:34
msgid "Suppress background images"
msgstr "Забрана на фонови картинки"

#: rc.cpp:35
msgid ""
"<b>Suppress background images</b><p>Selecting this option will prevent "
"Konqueror from loading background images.</p>"
msgstr ""
"<b>Забрана на фонови картинки</b><p> Konqueror "
"няма да показва фонови картинки. </p>"

