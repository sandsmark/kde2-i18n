#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr ""
"Помощна програма на KDE за отваряне на URL от "
"команден ред"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Показва наличните команди."

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Команда (виж. --commands)."

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Аргументи за командата."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Синтаксис:\n"

#: kfmclient.cc:77
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Отваря прозорец за преглед на 'url'.\n"
"            #  'url' може да бъде път \n"
"            #   или име на файл, например . или "
"поддиректория/\n"
"            #   Ако 'url' е изпуснат, ще бъде по "
"подразбиране използван $HOME.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # Ако е зададен 'mimetype', той ще бъде "
"използван за \n"
"            #   определяне на компонентата, "
"която ще стартира konqueror. Например, "
"задайте\n"
"            #   тип text/html (за WWW-страница), за да се "
"показва по-бързо.\n"
"\n"

#: kfmclient.cc:86
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Отваря прозорец, използвайки "
"зададен профил.\n"
"            #   'profile' се намира в "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' - това е незадължителен "
"параметър - URL, който вие искате да "
"отворите.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"            # Отваря меню с аксесоарите\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Опитва се да изпълни 'url'. Ако 'url' - е "
"обикновен\n"
"            #   URL, то той ще бъде отворен. Вие "
"можете да пропуснете\n"
"            #   'binding' . В този случай ще се "
"използва \n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   програма по подразбиреане. "
"Разбира се, URL може да бъде също \n"
"            #   адрес на документ или *.desktop файл.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   Така, например, вие можете да "
"монтирате устройство,\n"
"            #   използвайки параметър 'Mount default' "
"свързан за \n"
"            #   'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient move 'src' 'dest'\n"
"            # Премества URL 'src' в 'dest'.\n"
"            #   'src' може да бъде списък на URL.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient copy 'src' 'dest'\n"
"            # Копира URL 'src' в 'dest'.\n"
"            #   'src' може да бъде списък на URL.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"            # Пренарежда иконите на работното "
"място.\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"            # Препрочита конфигурацията на "
"konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"            # Препрочита конфигурацията на "
"работното място.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Примери:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Монтира CDROM\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Отваря файл, исползвайки "
"програма по подразбиране\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Отваря файл, използвайки Netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Отваря нов прозорец със зададен "
"URL\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Стартира emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Отваря директория с монтиран "
"CD-ROM\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // Отваря текущата директория. "
"Много удобно.\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"Профил %1 не е намерен\n"

#: kfmclient.cc:290
msgid ""
"Syntax Error: Not enough arguments\n"
msgstr ""
"Синтактична грешка. Няма достатъчно "
"аргументи.\n"

#: kfmclient.cc:295
msgid ""
"Syntax Error: Too many arguments\n"
msgstr ""
"Синтактична грешка. Твърде много "
"аргументи.\n"

#: kfmclient.cc:417
msgid ""
"Syntax Error: Unknown command '%1'\n"
msgstr ""
"Синтактична грешка: Неизвестна команда "
"'%1'\n"

