#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Промяна на паролата за този потребител."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Промяна на UNIX парола."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Текущо поддържане"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Вашата парола е променена."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Промяна на парола"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Моля, въведете вашата текуща парола."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Не можах да стартирам 'passwd'."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Не мога да намеря програмата 'passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Грешна парола! Опитайте още един път."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Вътрешна грешка: функцията "
"PasswdProcess::checkCurrent върна недопустимо "
"значение."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Въведете вашата нова парола."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr ""
"Вашата парола ще бъде съкратена до 8 "
"символа."

