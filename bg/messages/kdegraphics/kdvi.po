#: dvi_init.cpp:153
msgid "DVI file doesn't start with preamble."
msgstr "DVI файла не започва с встъпление."

#: dvi_init.cpp:155
msgid "Wrong version of DVI output for this program."
msgstr "Грешна версия на DVI изход за тази програма."

#: dvi_init.cpp:201
msgid "DVI file corrupted"
msgstr "DVI файла е повреден"

#: dvi_init.cpp:210
msgid "Wrong version of DVI output for this program"
msgstr "Грешна версия на DVI изход за тази програма."

#: dvi_init.cpp:221
msgid "Postamble doesn't begin with POST"
msgstr "Краят не започва с POST"

#: dvi_init.cpp:224
msgid "Postamble doesn't match preamble"
msgstr "Краят не съответства на встъплението"

#: dvi_init.cpp:234
msgid "Non-fntdef command found in postamble"
msgstr "Non-fntdef команда е намерена в края"

