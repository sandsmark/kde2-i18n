msgid "Login Script"
msgstr "Skrypt logowania"

#: accounts.cpp:371
msgid "Execute"
msgstr "Wykonaj"

#: accounts.cpp:372
msgid "Accounting"
msgstr "Koszty"

#: accounts.cpp:391
msgid ""
"You must enter an unique\n"
"account name"
msgstr ""
"Musisz wpisać\n"
"unikalną nazwę połączenia"

#: accounts.cpp:396
msgid "Login script has unbalanced loop Start/End"
msgstr "Skrypt logujący ma nierówną ilość początów/końców pętli"

#: accounts.cpp:410
