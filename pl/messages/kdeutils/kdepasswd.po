#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Zmień hasło dla wybranego użytkownika."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Zmienia hasło w Uniksie."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Autor"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Hasło zostało zmienione."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Zmień hasło"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Proszę wprowadzić aktualne hasło."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Porozumienie się z 'passwd' nie powiodło się."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Nie można było znaleźć programu 'passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Niewłaściwe hasło! Spróbuj jeszcze raz."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Błąd wewnętrzny: PasswdProcess zwrócił niewłaściwą "
"wartość::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Wprowadź nowe hasło."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Twoje hasło zostanie obcięte do 8 znaków."

