msgstr "sign(wartość)"

#: xml_doc.cc:345
msgid "sign(5) equals 1"
msgstr "sign(5) równe 1"

#: xml_doc.cc:346
msgid "sign(0) equals 0"
msgstr "sign(0) równe 0"

#: xml_doc.cc:347
msgid "sign(-5) equals -1"
msgstr "sign(-5) równe -1"

#: xml_doc.cc:349
