msgstr "INV(wartość)"

#: xml_doc.cc:351
msgid "INV(-5) equals 5"
msgstr "INV(-5) równe 5"

#: xml_doc.cc:352
msgid "INV(5) equals -5"
msgstr "INV(5) równe -5"

#: xml_doc.cc:353
msgid "INV(0) equals 0"
msgstr "INV(0) równe 0"

