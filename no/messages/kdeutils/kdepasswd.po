#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Endre passordet til denne brukeren."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passord"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Endre et Unix-passord."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Vedlikeholder"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Passordet ditt har blitt endret."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Endre passord"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Vennligst skriv inn ditt nåværende passord."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Samtale med \"passord\" mislykets."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Kunne ikke finne programmet \"passwd\"."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Feil passord! Vennligst prøv igjen."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Intern feil: ugyldig returverdi fra PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Vennligst skriv inn ditt nye passord."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Passordet ditt har blitt kuttet av til 8 tegn."

