#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"bruk: %s [-options] melding ...]\n"
"\n"

#: options.cpp:225
msgid ""
"where options include:\n"
msgstr ""
"hvor valg betyr:\n"

#: options.cpp:226
msgid ""
"  -title name             the title used on the message window\n"
msgstr ""
"  -title navn             tittelen brukt til meldingsvinduet\n"

#: options.cpp:227
msgid ""
"  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -file filnavn           filen som skal leses fra, , \"-\" for stdin\n"

#: options.cpp:228
msgid ""
"  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
"  -buttons streng         kommaseparert liste over "
"etiketter[:avsluttingskode]\n"

#: options.cpp:229
msgid ""
"  -default button         button to activate if Return is pressed\n"
msgstr ""
"  -default knapp          knapp som skal aktiveres hvis enter blir trykket\n"

#: options.cpp:230
msgid ""
"  -cancel button          button to activate if Escape is pressed\n"
msgstr ""
"  -cancel knapp           knapp som skal aktiveres hvis escape blir trykket\n"

#: options.cpp:231
msgid ""
"  -print                  print the button label when selected\n"
msgstr ""
"  -print                  skriv ut knappetiketten når den blir valg\n"

#: options.cpp:232
msgid ""
"  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -escape                 utvid \\n-,\\r- og \\t-tegn i melding\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -timeout sek            avslutt med status 0 etter \"sek\" sekunder\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"følgende valg er tillatt for kompatibilitet, men har ingen funksjon:\n"

#: options.cpp:236
msgid ""
"  -center\n"
msgstr ""
"  -center\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -nearmouse\n"
"\n"

