#: rc.cpp:1
msgid "HTML Export Dialog"
msgstr "HTML eksport dialog"

#: rc.cpp:2
msgid "Document Type"
msgstr "Dokumenttype"

#: rc.cpp:3
msgid "&HTML 4.01"
msgstr "&HTML 4.01"

#: rc.cpp:4
msgid "&XHTML 1.0"
msgstr "&XHTML 1.0"

#: rc.cpp:5
msgid "Mode"
msgstr "Tilstand"

#: rc.cpp:6
msgid "S&partan (Only document structure, no formatting!)"
msgstr "S&partansk (Kun dokumentstruktur, ingen formatering!)"

#: rc.cpp:7
msgid "&Direct (Formatting coded using HTML, no style sheets)"
msgstr "&Direkte (Formatering kodet ved brug af HTML, ingen stilark)"

#: rc.cpp:8
msgid "&Style (Formatting coded using CSS2, no style sheets)"
msgstr "&Stil (Formatering kodet ved brug af CSS2, ingen stilark)"

#: rc.cpp:9
msgid "&Full Power (As much as possible using CSS2, less compatible)"
msgstr "&Fuld kraft (Så meget som muligt ved brug af CSS2, mindre kompatibel)"

#: rc.cpp:10
msgid "HTML Import Dialog"
msgstr "HTML import dialog"

#: rc.cpp:11
msgid "Character Set"
msgstr "Tegnsæt"

#: rc.cpp:12
msgid "Hint For Character Set:"
msgstr "Vink angående tegnsæt:"

#: rc.cpp:13
msgid "&Hint"
msgstr "&Vink"

#: rc.cpp:14
msgid "Use hint only as &fall back!"
msgstr "Brug kun vink som &nødløsning!"

#: rc.cpp:15
msgid "Force the use of the hint! (For buggy files!)"
msgstr "Påtving brug af vink (For filer med problemer!)"

#: rc.cpp:16
msgid "Do &not use the hint! (Recommanded!)"
msgstr "Brug &ikke vinket! (Anbefalet!)"
