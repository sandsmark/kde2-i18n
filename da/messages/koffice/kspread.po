msgstr "IMSUB(1,2;\"1i\") giver \"1,2-i\""

#: xml_doc.cc:569
msgid "The IMPRODUCT() returns the product of several complex numbers of form x+yi."
msgstr "IMPRODUCT() giver produktet af adskillige komplekse tal af formen x+yi."

#: xml_doc.cc:570
msgid "IMPRODUCT(value;value;...)"
msgstr "IMPRODUCT(værdi;værdi;...)"

#: xml_doc.cc:571
msgid "IMPRODUCT(1.2;\"3.4+5i\") returns \"4.08+6i\""
msgstr "IMPRODUCT(1,2;\"3,4+5i\") giver \"4,08+6i\""

#: xml_doc.cc:572
msgid "IMPRODUCT(1.2;\"1i\") returns \"+1.2i\""
msgstr "IMPRODUCT(1,2;\"1i\") giver \"+1,2i\""

#: xml_doc.cc:574
msgid ""
"The IMCONJUGATE(complex number) returns the conjugate of a complex number of "
"form x+yi."
msgstr ""
"IMCONJUGATE(kompleks tal) giver den konjugerede af et komplekst tal af "
"formen x+yi."

#: xml_doc.cc:575
msgid "IMCONJUGATE(complex number)"
msgstr "IMCONJUGATE(kompleks tal)"

#: xml_doc.cc:576
