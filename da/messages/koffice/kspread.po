msgstr "IMDIV(\"12+i\";\"12-i\") giver \"0.986207+0.16551i\""

#: xml_doc.cc:560
msgid "The IMSUB() returns the difference of several complex numbers of form x+yi."
msgstr "IMSUB() giver forskellen mellem adskillige komplekse tal af formen x+yi."

#: xml_doc.cc:561
msgid "IMSUB(value;value;...)"
msgstr "IMSUB(værdi;værdi;...)"

