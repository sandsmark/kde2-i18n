#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"brug: %s [-options] besked ...]\n"
"\n"

#: options.cpp:225
msgid ""
"where options include:\n"
msgstr ""
"hvor valg betyder:\n"

#: options.cpp:226
msgid ""
"  -title name             the title used on the message window\n"
msgstr ""
"  -titel navn             titlen brugt til beskedsvinduet\n"

#: options.cpp:227
msgid ""
"  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -file filnavn           filen som skal læses fra, , \"-\" for stdin\n"

#: options.cpp:228
msgid ""
"  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
"  -buttons streng         kommasepareret liste over "
"etiketter[:afslutningskode]\n"

#: options.cpp:229
msgid ""
"  -default button         button to activate if Return is pressed\n"
msgstr ""
"  -standard knap          knap som skal aktiveres hvis enter bliver trykket\n"

#: options.cpp:230
msgid ""
"  -cancel button          button to activate if Escape is pressed\n"
msgstr ""
"  -annullér knap           knap som skal aktiveres hvis escape bliver "
"trykket\n"

#: options.cpp:231
msgid ""
"  -print                  print the button label when selected\n"
msgstr ""
"  -print                  udskriv knapetiketten når den bliver valgt\n"

#: options.cpp:232
msgid ""
"  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -escape                 udvid \\n-,\\r- og \\t-tegn i besked\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -timeout sek            afslut med status 0 efter \"sek\" sekunder\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"følgende valg er tilladt for kompatibilitet, men har ingen funktion:\n"

#: options.cpp:236
msgid ""
"  -center\n"
msgstr ""
"  -center\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -nearmouse\n"
"\n"

