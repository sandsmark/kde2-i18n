#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Ændr kodeord for denne bruger."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE kodeord"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Ændrer et unix kodeord."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Vedligeholder"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Dit kodeord er ændret."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Ændr kodeord"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Indtast dit nuværende kodeord."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Konversation med `passwd' mislykkedes."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Kunne ikke finde programmet `passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Forkert kodeord. Prøv igen!"

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Indre fejl: illegal returværdi fra PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Indtast venligst dit nye kodeord."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Dit kodeord vil blive afkortet til 8 tegn."

