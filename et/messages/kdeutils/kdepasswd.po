#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Muuda selle kasutaja parooli."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE parool"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Muudab Unix'i parooli."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Hooldaja"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Sinu parool on muudetud."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Muuda parool"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Palun sisesta oma praegune parool."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Suhtlemine käsuga 'passwd' ebaõnnestus."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Programmi 'passwd' pole võimalik leida."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Parool on vale! Palun proovi uuesti."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Sisemine viga: vigane tagastatud väärtus funktsioonist "
"PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Palun sisesta oma uus parool."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Sinu parool muudetakse 8 sümboli pikkuseks."

