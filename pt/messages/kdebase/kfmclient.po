#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr "Uma ferramenta do KDE para abrir URLs da linha de comandos"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Mostrar os comandos disponíveis:"

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Comando (ver --commands)."

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Argumentos para o comando."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Sintaxe:\n"

#: kfmclient.cc:77
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url' ['tipo mime']\n"
"            # Abre uma janela a mostrar a 'url'.\n"
"            #  'url' pode ser uma directoria relativa\n"
"            #   ou um ficheiro, tal como . ou directoria/\n"
"            #   Se 'url' for omitida, é utilizado $HOME.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # Se o 'tipo mime' for especificado, será utilizado para "
"determinar\n"
"            #   o componente que o konqueror deve utilizar. Por exemplo,\n"
"            #   selecciona text/html para uma página da web, para a "
"fazeres\n"
"            #   aparecer mais depressa\n"
"\n"

#: kfmclient.cc:86
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'perfil' ['url']\n"
"            # Abre uma janela utilizando um determinado perfil.\n"
"            #   'perfil' é um ficheiro em "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' é um URL opcional a abrir.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"            # Abre o menu de propriedades\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tenta executar 'url'. 'url' pode ser uma URL\n"
"            #   normal e esta URL será aberta. Podes omitir\n"
"            #   'binding'. Neste caso a ligação por omissão\n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   é tentada. É claro que o URL pode ser o URL de um\n"
"            #   documento, ou pode ser um ficheiro *.desktop.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   Deste modo podes por exemplo montar um dispositivo\n"
"            #   passando 'Mount default' como parâmetro a\n"
"            #   'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient move 'src' 'dest'\n"
"            # Move a URL 'src' para 'dest'.\n"
"            #   'src' pode ser uma lista de URLs.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copia a URL 'src' para 'dest'.\n"
"            #   'src' pode ser uma lista de URLs.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"            # Reorganiza todos os ícones no ambiente de trabalho\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"            # Voltar a carregar a configuração do konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"            # Voltar a carregar a configuração do kdesktop.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Exemplo:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Monta o CDROM\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Abre o ficheiro com a aplicação associada\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Abre o ficheiro com o netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Abre uma nova janela com a URL\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Arranca o emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Abre a directoria do CD-ROM\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // Abre a directoria actual. É muito útil.\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"O perfil %1 não foi encontrado\n"

