#: main.cpp:45
msgid "KFM Exec - Opens remote files, watches modifications, asks for upload"
msgstr ""
"KFM Exec - Abre ficheiros remotos, verifica as modificações destes e pede "
"a transferência"

#: main.cpp:51
msgid "Command to execute."
msgstr "Comando a executar."

#: main.cpp:52
msgid "URL(s) or local file used for 'command'."
msgstr "URL(s) ou ficheiros locais usados para 'comando'."

#: main.cpp:68
msgid ""
"'command' expected.\n"
msgstr ""
"Era esperado 'comando'.\n"

#: main.cpp:89
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"O URL %1\n"
"é inválido"

#: main.cpp:196
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"O ficheiro\n"
"%1\n"
"foi modificado.\n"
"Queres enviar as alterações?"

#: main.cpp:197
msgid "File changed"
msgstr "Ficheiro alterado"

#: main.cpp:256
msgid "KFMExec"
msgstr "KFMExec"

#: main.cpp:271
msgid ""
"Syntax Error:\n"
"kfmexec command [URLs ....]"
msgstr ""
"Erro de Sintaxe:\n"
"kfmexec comando [URLs ....]"

