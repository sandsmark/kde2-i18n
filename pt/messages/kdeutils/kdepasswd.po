#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Mudar a senha deste utilizador."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "Passwd do KDE"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Altera uma senha de acesso Unix."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Responsável pela manutenção"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "A tua senha foi modificada."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Modificar senha"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Por favor, indica a tua senha actual."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "A negociação com o `passwd' falhou."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Não consegui encontrar o programa `passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Senha errada! Tenta novamente."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Erro interno: o valor de retorno de PasswdProcess::checkCurrent não foi o "
"esperado."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Por favor, insere a nova senha."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "A tua senha será limitada a 8 caracteres."

