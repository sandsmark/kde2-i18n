msgid "All below warnings combined:"
msgstr "Todos os avisos abaixo combinados:"

#: plugins/gccoptions/gccoptionsplugin.cpp:329
msgid "<qt>Warn if an array subscript has type <i>char</i>.</qt>"
msgstr "<qt>Avisa se o subscrito dum vector tem o tipo <i>char</i>.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:331
msgid "<qt>Warn when a comment-start sequence /* appears inside a comment.</qt>"
msgstr "<qt>Avisa quando uma sequência /* aparece dentro dum comentário.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:333
msgid "<qt>Check calls to <i>printf()</i>, <i>scanf()</i> etc.</qt>"
msgstr "<qt>Verifica as chamadas a <i>printf()</i>, <i>scanf()</i> etc.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:335
msgid "<qt>Warn when a declaration does not specify a type.</qt>"
msgstr "<qt>Avisa quando uma declaração não especifica o seu tipo.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:338
msgid "<qt>Issue a warning when a non-declared function is used.</qt>"
msgstr "<qt>Mostra um aviso quando uma função não declarada é usada.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:341
msgid "<qt>Issue an error when a non-declared function is used.</qt>"
msgstr "<qt>Mostra um erro quando uma função não declarada é usada.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:343
msgid "<qt>Warn if the type of <i>main()</i> is suspicious.</qt>"
msgstr "<qt>Avisa quando o tipo do <i>main()</i> é suspeito.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:345
msgid "<qt>Warn when multicharacter constants are encountered.</qt>"
msgstr "<qt>Avisa quando são encontradas constantes multi-carácter.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:347
msgid "<qt>Warn when parentheses are omitted in certain context.</qt>"
msgstr "<qt>Avisa quando os parêntesis são omitidos em certos contextos.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:349
msgid "<qt>Warn when a function without explicit return type is defined.</qt>"
msgstr ""
"<qt>Avisa quando uma função sem tipo de retorno explícito é "
"definida.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:351
msgid "<qt>Warn when trigraphs are encountered.</qt>"
msgstr "<qt>Avisa quando são encontrados trigrafos.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:353
msgid "<qt>Warn when a variable is declared but not used.</qt>"
msgstr "<qt>Avisa quando uma variável é declarada mas não é usada.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:355
msgid "<qt>Warn when a variable is used without being initialized before.</qt>"
msgstr "<qt>Avisa quando uma variável é usada sem ter sido inicializada antes.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:357
msgid "<qt>Warn when an unknown #pragma statement is encountered.</qt>"
msgstr "<qt>Avisa quando uma directiva #pragma desconhecida é encontrada.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:360
msgid ""
"<qt>Warn when the order of member initializers is different from\n"
"the order in the class declaration.</qt>"
msgstr ""
"<qt>"
"Avisa quando a ordem dos inicializadores do membro é diferente da ordem na "
"declaração da classe.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:395
msgid "<qt>Set options not included in -Wall which are very specific.</qt>"
msgstr ""
"<qt>Activa as opções não incluídas no -Wall por serem muito "
"específicas.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:397
msgid ""
"<qt>Warn about certain constructs that behave differently\n"
"in traditional and ANSI C.</qt>"
msgstr ""
"<qt>Avisa sobre certas construções\n"
"que se comportam de modo diferente no\n"
"C tradicional e no ANSI C.</qt>"

