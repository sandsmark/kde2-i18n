#: main.cpp:45
msgid "KFM Exec - Opens remote files, watches modifications, asks for upload"
msgstr ""
"KFM Exec - Abre archivos remotos, vigila las modificaciones y pregunta antes "
"de enviar información."

#: main.cpp:51
msgid "Command to execute."
msgstr "Comando a ejecutar."

#: main.cpp:52
msgid "URL(s) or local file used for 'command'."
msgstr "URL(s) o archivo local a usar para el comando 'command'."

#: main.cpp:68
msgid ""
"'command' expected.\n"
msgstr ""
"Se esperaba el comando 'command'.\n"

#: main.cpp:89
msgid ""
"The URL %1\n"
"is malformed"
msgstr ""
"El formato de la URL %1\n"
"es erróneo"

#: main.cpp:196
msgid ""
"The file\n"
"%1\n"
"has been modified.\n"
"Do you want to upload the changes?"
msgstr ""
"El archivo\n"
"%1\n"
"ha sido modificado.\n"
"¿Desea enviar los cambios?"

#: main.cpp:197
msgid "File changed"
msgstr "Archivo cambiado"

#: main.cpp:256
msgid "KFMExec"
msgstr "KFMExec"

#: main.cpp:271
msgid ""
"Syntax Error:\n"
"kfmexec command [URLs ....]"
msgstr ""
"Error de sintaxis:\n"
"kfmexec comando [URLs ...]"

