#: rc.cpp:59
msgid "You can enter the size of the right border here."
msgstr "Puede introducir el tamaño del borde derecho aquí."

#: rc.cpp:60
msgid "&Top:"
msgstr "Su&perior:"

#: rc.cpp:61
msgid "You can enter the size of the top border here."
msgstr "Puede introducir el tamaño del borde superior aquí."

#: rc.cpp:62
msgid "&Left:"
msgstr "&Izquierdo:"

#: rc.cpp:63
msgid "You can enter the size of the left border here."
msgstr "Puede introducir el tamaño del borde izquierdo aquí."

#: rc.cpp:64
msgid "&Right:"
msgstr "&Derecho:"

#: rc.cpp:65
msgid "&Bottom:"
msgstr "&Inferior:"

#: rc.cpp:66
