#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Cambiar la contraseña de este usuario."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "Passwd de KDE"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Cambia una contraseña Unix."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Encargado"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Su contraseña ha sido modificada."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Cambiar contraseña"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Por favor introduzca su contraseña actual."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Comunicación con 'passwd' fallida."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "No se encontró el programa 'passwd'"

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "¡Contraseña incorrecta! Intentelo de nuevo, por favor."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Error interno: valor de retorno ilegal de PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Por favor introduzca su nueva contraseña."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Su contraseña será recortada a 8 caracteres."

