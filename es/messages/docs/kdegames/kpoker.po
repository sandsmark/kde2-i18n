msgid "Four cards of the same rank"
msgstr "Cuatro cartas del mismo tipo"

#: index.docbook:176 index.docbook:237
msgid "Full House"
msgstr "Full"

#: index.docbook:176
msgid "Three cards of one rank and two cards of another"
msgstr "Tres cartas de un tipo y dos cartas de otro"

#: index.docbook:176 index.docbook:237
msgid "Straight"
msgstr "Escalera"

#: index.docbook:176
msgid "Five cards of consecutive rank including the combo ace-2-3-4-5"
msgstr "Cinco cartas numeradas consecutivamente incluyendo la combinación as-2-3-4-5"

#: index.docbook:176 index.docbook:237
