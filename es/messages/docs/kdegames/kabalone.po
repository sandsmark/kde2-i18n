msgid ""
"<shortcut> <keycombo><keycap>PgDown</keycap></keycombo> </shortcut> "
"<guimenu><accel>G</accel>ame</guimenu><guimenuitem><accel>F</accel>"
"orward</guimenuitem>"
msgstr ""
"<shortcut> <keycombo><keycap>Pg Down</keycap></keycombo> </shortcut> "
"<guimenu><accel>P</accel>artida</guimenu><guimenuitem>A<accel>d</accel>"
"elante</guimenuitem>"

