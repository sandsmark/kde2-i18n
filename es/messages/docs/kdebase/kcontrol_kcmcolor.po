msgstr "<member>CDE</member>"

#: index.docbook:100
msgid "Dark Blue,"
msgstr "Azul oscuro,"

#: index.docbook:100
msgid "Desert red,"
msgstr "Rojo desierto,"

#: index.docbook:100
msgid "Digital CDE,"
msgstr "CDE de Digital,"

#: index.docbook:100
msgid "Pale gray,"
msgstr "Gris pálido,"

#: index.docbook:100
msgid "Pumpkin,"
msgstr "Calabaza,"

#: index.docbook:100
msgid "Solaris CDE,"
