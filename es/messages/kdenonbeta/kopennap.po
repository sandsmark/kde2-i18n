msgid " /del username : remove user 'username' from friend list"
msgstr " /del usu : borrar el usuario 'usu' de la lista de amigos"

#: chatwindow.cpp:176
msgid "/talk user     : open up a private talk window for user"
msgstr "/talk usu     : iniciar una charla privadacon el usuario usu"

#: chatwindow.cpp:178 talkwindow.cpp:118
msgid " /save        : prompt to save contents of the window"
msgstr " /save        : solicitar si debe salvarse el contenido de la ventana"

#: chatwindow.cpp:181
msgid "/kill username : kill username"
msgstr "/kill usu : cerrar la sesión con el usuario usu"

#: chatwindow.cpp:182
msgid "/kick username : kick username off this channel"
msgstr "/kick usuario : sacar al usuario de este canal"

#: chatwindow.cpp:186
msgid "Anything else not starting with / is sent directly to the channel"
msgstr "Cualquier otra cosa que no empiece con / es enviada directamente al canal"

