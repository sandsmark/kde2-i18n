#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"uso: %s [-opciones] [mensaje ...]\n"
"\n"

#: options.cpp:225
msgid ""
"where options include:\n"
msgstr ""
"donde las opciones incluyen:\n"

#: options.cpp:226
msgid ""
"  -title name             the title used on the message window\n"
msgstr ""
"  -título             el título utilizado en la ventana de mensajes\n"

#: options.cpp:227
msgid ""
"  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -nombre de archivo          archivo desde el que se lee el mensaje, \"-\" "
"para stdin\n"

#: options.cpp:228
msgid ""
"  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
" -cadena de botones          lista de etiquetas separada por comas[:código "
"de salida]\n"

#: options.cpp:229
msgid ""
"  -default button         button to activate if Return is pressed\n"
msgstr ""
"  -botón predeterminado         botón para activar si se pulsa Intro\n"

#: options.cpp:230
msgid ""
"  -cancel button          button to activate if Escape is pressed\n"
msgstr ""
"  -botón de cancelación          botón para activar si se pulsa Escape\n"

#: options.cpp:231
msgid ""
"  -print                  print the button label when selected\n"
msgstr ""
"  -imprimir                  imprimir la etiqueta del botón cuando se "
"seleccione\n"

#: options.cpp:232
msgid ""
"  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -escape                 expandir  los caracteres \\n,\\r, y \\t en el "
"mensaje\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -segundos de tiempo de espera           salir con estado 0 después de "
"\"secs\" segundos\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"Las siguientes opciones se permiten para compatibilidad, pero no son "
"funcionales:\n"

#: options.cpp:236
msgid ""
"  -center\n"
msgstr ""
"  -centrar\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -cerca del ratón\n"
"\n"

#: options.cpp:239
#, c-format
msgid ""
"%s version %s\n"
msgstr ""
"%s versión %s\n"

#: options.cpp:240
#, c-format
msgid ""
"by %s\n"
msgstr ""
"por %s\n"

