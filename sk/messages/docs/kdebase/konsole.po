msgid "The <guimenu>Help</guimenu> menu"
msgstr "Menu <guimenu>Pomocník</guimenu>"

#: index.docbook:568
msgid ""
"<shortcut> <keycap>F1</keycap></shortcut><guimenu>Help</guimenu> "
"<guimenuitem><accel>C</accel>ontents...</guimenuitem>"
msgstr ""
