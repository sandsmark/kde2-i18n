msgid "IMSQRT(string)"
msgstr "IMSQRT(reťazec)"

#: xml_doc.cc:529
msgid "IMSQRT(\"1+i\") returns \"1.09868+0.45509i\""
msgstr "IMSQRT(\"1+i\") vracia \"1.09868+0.45509i\""

#: xml_doc.cc:530
msgid "IMSQRT(\"1.2i\") returns \"0.774597+0.774597i\" "
msgstr "IMSQRT(\"1.2i\") vracia \"0.774597+0.774597i\" "

#: xml_doc.cc:532
msgid "Power"
msgstr "Exponent"

