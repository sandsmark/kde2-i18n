#: xml_doc.cc:543
msgid "IMSUM(value;value;...)"
msgstr "IMSUM(hodnoty;hodnoty;...)"

#: xml_doc.cc:544
msgid "IMSUM(1.2;\"3.4+5i\") returns \"4.6+5i\""
msgstr "IMSUM(1.2;\"3.4+5i\") vracia \"4.6+5i\""

#: xml_doc.cc:545
msgid "IMSUM(1.2;\"1i\") returns \"1.2+i\""
msgstr "IMSUM(1.2;\"1i\") vracia \"1.2+i\""

#: xml_doc.cc:551
msgid "The IMDIV() returns the division of several complex numbers of form x+yi."
msgstr "Funkcia IMDIV() vracia podiel niekoľkých komplexných čísel v tvare x+yi."

#: xml_doc.cc:552
msgid "IMDIV(value;value;...)"
msgstr "IMDIV(hodnoty;hodnoty;...)"

#: xml_doc.cc:553
msgid "IMDIV(1.2;\"3.4+5i\") returns \"0.111597-0.164114i\""
msgstr "IMDIV(1.2;\"3.4+5i\") vracia \"0.111597-0.164114i\""

#: xml_doc.cc:554
msgid "IMDIV(\"12+i\";\"12-i\") returns \"0.986207+0.16551i\""
msgstr "IMDIV(\"12+i\";\"12-i\") vracia \"0.986207+0.16551i\""

#: xml_doc.cc:560
msgid "The IMSUB() returns the difference of several complex numbers of form x+yi."
msgstr ""
"Funkcia IMSUB() vracia rozdiel niekoľkých komplexných čísel v tvare "
"x+yi."

#: xml_doc.cc:561
msgid "IMSUB(value;value;...)"
msgstr "IMSUB(hodnoty;hodnoty;...)"

#: xml_doc.cc:562
msgid "IMSUB(1.2;\"3.4+5i\") returns \"-2.2-5i\""
msgstr "IMSUB(1.2;\"3.4+5i\") vracia \"-2.2-5i\""

#: xml_doc.cc:563
msgid "IMSUB(1.2;\"1i\") returns \"1.2-i\""
msgstr "IMSUB(1.2;\"1i\") vracia \"1.2-i\""

#: xml_doc.cc:569
msgid "The IMPRODUCT() returns the product of several complex numbers of form x+yi."
msgstr ""
"Funkcia IMPRODUCT() vracia súčin niekoľkých komplexných čísel v tvare "
"x+yi."

#: xml_doc.cc:570
msgid "IMPRODUCT(value;value;...)"
msgstr "IMPRODUCT(hodnoty;hodnoty;...)"

#: xml_doc.cc:571
msgid "IMPRODUCT(1.2;\"3.4+5i\") returns \"4.08+6i\""
msgstr "IMPRODUCT(1.2;\"3.4+5i\") vracia \"4.08+6i\""

#: xml_doc.cc:572
msgid "IMPRODUCT(1.2;\"1i\") returns \"+1.2i\""
msgstr "IMPRODUCT(1.2;\"1i\") vracia \"+1.2i\""

#: xml_doc.cc:574
msgid ""
"The IMCONJUGATE(complex number) returns the conjugate of a complex number of "
"form x+yi."
msgstr ""
"Funkcia IMCONJUGATE(komplexné číslo) vracia číslo komplexne združené "
"v tvare x+yi."

#: xml_doc.cc:575
msgid "IMCONJUGATE(complex number)"
msgstr "IMCONJUGATE(komplexné číslo)"

#: xml_doc.cc:576
msgid "IMCONJUGATE(\"1.2+5i\") returns \"1.2-5i\""
msgstr "IMCONJUGATE(\"1.2+5i\") vracia \"1.2-5i\""

#: xml_doc.cc:577
msgid "IMCONJUGATE(\"-i\") returns \"i\""
msgstr "IMCONJUGATE(\"-i\") vracia \"i\""

#: xml_doc.cc:578
msgid "IMCONJUGATE(\"12\") returns \"12\""
msgstr "IMCONJUGATE(\"12\") vracia \"12\""

#: xml_doc.cc:580
