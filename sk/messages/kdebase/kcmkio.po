msgstr "Identifikácia špecifická pre server/doménu"

#: useragentdlg.cpp:126
msgid "Site/domain name"
msgstr "Meno serveru/domény"

#: useragentdlg.cpp:127
msgid "UserAgent"
msgstr "Agent"

#: useragentdlg.cpp:128
msgid "Alias"
msgstr "Alias"

