#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Ŝanĝu la pasvorton de tiu uzanto."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE-pasvorto"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "nĝas uniksan pasvorton."

#: kdepasswd.cpp:35
msgid "Maintainer"
