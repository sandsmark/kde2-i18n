#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Promenite lozinku ovog korisnika."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Menja Unix lozinku."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Programer koji održava aplikaciju"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Vaša lozinka je promenjena."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Promenite lozinku"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Molim vas da unesete vašu trenutnu lozinku."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Razmena podataka sa programom 'passwd' nije uspela."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Ne mogu da pronađem program 'passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Neispravna lozinka! Molim vas da pokušate ponovo."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Interna greška: funkcija 'PasswdProcess::checkCurrent' vratila je "
"nedozvoljenu vrednost."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Molim vas da unesete novu lozinku."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Vaša lozinka će biti skraćena na 8 znakova."

