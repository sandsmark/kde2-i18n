msgstr "IMARGUMENT(\"-i\") palauttaa -1.57079633 "

#: xml_doc.cc:584
msgid "IMARGUMENT(\"12\") returns \"#Div/0\""
msgstr "IMARGUMENT(\"12\") palauttaa \"#Div/0\""

#: xml_doc.cc:586
