msgstr "IMSUB(1.2;\"3.4+5i\") palauttaa \"-2.2-5i\""

#: xml_doc.cc:563
msgid "IMSUB(1.2;\"1i\") returns \"1.2-i\""
msgstr "IMSUB(1.2;\"1i\") palauttaa \"1.2-i\""

#: xml_doc.cc:569
