msgid "HTML Import Dialog"
msgstr "HTML-tuonti"

#: rc.cpp:11
msgid "Character Set"
msgstr "Merkkijoukko"

#: rc.cpp:12
msgid "Hint For Character Set:"
msgstr "Vihje merkkijoukolle:"

#: rc.cpp:13
msgid "&Hint"
msgstr "&Vihje"

#: rc.cpp:14
msgid "Use hint only as &fall back!"
msgstr "Käytä vihjettä vain &tarvittaessa!"

#: rc.cpp:15
msgid "Force the use of the hint! (For buggy files!)"
msgstr "Pakota vihjeen käyttö! (Virheellisille tiedostoille!)"

