msgstr "HTML-vienti"

#: rc.cpp:2
msgid "Document Type"
msgstr "Asiakirjan tyyppi"

#: rc.cpp:3
msgid "&HTML 4.01"
msgstr "&HTML 4.01"

#: rc.cpp:4
msgid "&XHTML 1.0"
msgstr "&XHTML 1.0"

#: rc.cpp:5
msgid "Mode"
msgstr "Tila"

#: rc.cpp:6
msgid "S&partan (Only document structure, no formatting!)"
msgstr "&Karu (Vain asiakirjan sisältö, ei muotoilua!)"

