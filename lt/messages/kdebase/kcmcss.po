msgid "48"
msgstr "48"

#: rc.cpp:60
msgid "64"
msgstr "64"

#: rc.cpp:61
msgid "Base Font Si&ze:"
msgstr "&Bazinis šrifto dydis:"

#: rc.cpp:62
msgid "&Use same size for all elements"
msgstr "Na&udoti tą patį dydį visiems elementams"

#: rc.cpp:63
msgid ""
"<b>Use same size for all elements</b><p>Select this option to override "
"custom font sizes in favor of the base font size.  All fonts will be "
"displayed in the same size.</p>"
msgstr ""

#: rc.cpp:64
msgid "Preview"
msgstr "Peržiūra"

#: rc.cpp:65
msgid ""
"<qt>\n"
"\n"
"<h1>Heading 1</h1><br/>\n"
"<h2>Heading 2</h2><br/>\n"
"<h3>Heading 3</h3><br/>\n"
"\n"
"<p>User defined stylesheets allow increased\n"
"accessibility for visually handicapped\n"
"people.</p>\n"
"\n"
"</qt>"
msgstr ""
"<qt>\n"
"\n"
"<h1>Antraštė 1</h1><br/>\n"
"<h2>Antraštė 2</h2><br/>\n"
"<h3>Antraštė 3</h3><br/>\n"
"\n"
"<p>Vartotojo parinktos stilių lentelės\n"
"padidina prieinamumą sutrikusio regėjimo\n"
"žmonėms.</p>\n"
"\n"
"</qt>"

