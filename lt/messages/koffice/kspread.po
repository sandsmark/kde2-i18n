msgid "NAND(value;value;...)"
msgstr "NAND(reikšmė;reikšmė;...)"

#: xml_doc.cc:617
msgid "NAND(True;False;False) returns True"
msgstr "NAND(True;False;False) gražins True"

#: xml_doc.cc:618
msgid "NAND(True;True) returns False"
msgstr "NAND(True;True) gražins False"

#: xml_doc.cc:624
