msgid "log(0.8) equals -0.09691001"
msgstr "log(0.8) lygu -0.09691001"

#: xml_doc.cc:442
msgid "log(0) equals -inf."
msgstr "log(0) lygu -inf."

#: xml_doc.cc:444
