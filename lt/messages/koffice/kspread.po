msgid "fabs(x)"
msgstr "fabs(x)"

#: xml_doc.cc:461
msgid "fabs(12.5) equals 12.5"
msgstr "fabs(12.5) lygu 12.5"

#: xml_doc.cc:462
msgid "fabs(-12.5) equals 12.5"
msgstr "fabs(-12.5) lygu 12.5"

