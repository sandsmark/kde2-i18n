msgid "IMREAL(\"1.2+3.4i\") returns 1.2"
msgstr "IMREAL(\"1.2+3.4i\") riturna 1.2"

#: xml_doc.cc:505
msgid "IMREAL(\"1.2i\") returns 0 "
msgstr "IMREAL(\"1.2i\") restituisce 0 "

#: xml_doc.cc:507
msgid "The IMCOS(string) returns the cosine of a complex number."
msgstr "La funzione IMCOS(stringa) restituisce il coseno di un numero complesso."

#: xml_doc.cc:508
msgid "IMCOS(string)"
msgstr "IMCOS(stringa)"

#: xml_doc.cc:509
msgid "IMCOS(\"1+i\") returns \"0.83373-0.988898i\""
msgstr "IMCOS(\"1+i\") riturna \"0.83373-0.988898i\""

#: xml_doc.cc:510
msgid "IMCOS(\"12i\") returns 81 377.4 "
msgstr "IMCOS(\"12i\") restituisce 81377.4 "

#: xml_doc.cc:512
msgid "The IMSIN(string) function returns the sine of a complex number."
msgstr "La funzione IMSIN(stringa) restituisce il seno di un numero complesso."

#: xml_doc.cc:513
msgid "IMSIN(string)"
msgstr "IMSIN(stringa)"

