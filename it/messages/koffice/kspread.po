msgid "IMSIN(\"1+i\") returns \"1.29846+0.634964i\""
msgstr "IMSIN(\"1+i\") restituisce \"1.29846+0.634964i\""

#: xml_doc.cc:515
msgid "IMSIN(\"1.2\") returns -0.536573 "
msgstr "IMSIN(\"1.2\") restituisce -0.536573 "

#: xml_doc.cc:517
msgid "The IMEXP(string) returns the exponential of a complex number."
msgstr "La funzione IMEXP(stringa) restituisce l'esponenziale di un numero complesso."

#: xml_doc.cc:518
msgid "IMEXP(string)"
msgstr "IMEXP(stringa)"

#: xml_doc.cc:519
msgid "IMEXP(\"2-i\") returns \"3.99232-6.21768i\""
msgstr "IMEXP(\"2-i\") restituisce \"3.99232-6.21768i\""

#: xml_doc.cc:520
