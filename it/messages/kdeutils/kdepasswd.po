#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Cambia la password di questo utente."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "passwd per KDE"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Cambia una password Unix."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Manutentore"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "La tua password è stata cambiata."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Cambia password"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Per piacere, immetti la tua password attuale."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Conversazione con \"passwd\" fallita."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Impossibile trovare il programma \"passwd\"."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Password sbagliata! Per piacere, prova ancora."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Errore interno: PasswdProcess::checkCurrent ha restituito un valore non "
"valido."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Per piacere, immetti la tua nuova password."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "La tua password sarà troncata a 8 caratteri."

