#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr "Accessorio di KDE per aprire URL dalla linea di comando"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Mostra i comandi disponibili."

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Comando (vedi --commands)"

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Argomenti per il comando."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Sintassi:\n"

#: kfmclient.cc:77
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Apre una finestra che mostra l'url.\n"
"            #  'url' può essere un percorso\n"
"            #   o un nome file, come . o una sottodirectory/\n"
"            #   Se 'url' viene omesso, $HOME viene usato al suo posto.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""
"            # Se viene specificato 'mimetype' sarà utlizzato per "
"determinare\n"
"            #   il componente che konqueror dovrà utilizzare. Per esmpio, "
"impostalo a\n"
"            #   text/html per una web page, se vuoi farla comparire più "
"velocemente\n"
"\n"

#: kfmclient.cc:86
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Apre una finestra usando il profilo indicato.\n"
"            #   `profile' è un file in "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' è una URL opzionale da aprire.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"            # Apre un menu delle proprietà\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Cerca di eseguire `url'. `url' può essere una URL\n"
"            #   normale, questa URL sarà aperta. Puoi tralasciare\n"
"            #   'binding'. In questo caso verrà provato il binding\n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   predefinito. `url' può essere l'URL di un\n"
"            #   documento, o di un file *.desktop.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   In questo modo puoi ad esempio montare un\n"
"            #   dispositivo passando `Mount default' come\n"
"            #   binding per 'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient move 'src' 'dest'\n"
"            # Sposta l'URL 'src' su 'dest'.\n"
"            #   'src' può essere una lista di URL.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copia l'URL 'src' su 'dest'.\n"
"            #   'src' può essere una lista di URL.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"            # Ridispone tutte le icone sul desktop.\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"            # Rilegge la configurazione di konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"            # Rilegge la configurazione di kdesktop.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Esesmpi:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Monta il CD-ROM\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Apre il file con il binding predefinito\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Apre il file con netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Apre una nuova finestra con URL\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Avvia emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Apre la directory in cui è montato il CD-ROM\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // Apre la directory corrente. Molto utile\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"Profilo %1 non trovato\n"

#: kfmclient.cc:290
msgid ""
"Syntax Error: Not enough arguments\n"
msgstr ""
"Errore di sintassi: Non abbastanza argomenti\n"

#: kfmclient.cc:295
msgid ""
"Syntax Error: Too many arguments\n"
msgstr ""
"Errore di sintassi: Troppi argomenti\n"

#: kfmclient.cc:417
msgid ""
"Syntax Error: Unknown command '%1'\n"
msgstr ""
"Errore di sintassi: Comando '%1' sconosciuto\n"

