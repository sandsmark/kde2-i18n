#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Endra passordet til denne brukaren."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Endrar eit Unix-passord."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Vedlikehaldar"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Passordet ditt er endra."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Endra passord"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Oppgi ditt noverande passord."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Mislukka kommunikasjon med «passwd»."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Fann ikkje programmet «passwd»."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Feil passord. Prøv igjen."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Intern feil: ulovleg returverdi frå PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Oppgi ditt nye passord."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Passordet ditt vert korta ned til åtte teikn."

