#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Ändra lösenord för den här användaren."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE-passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Ändrar ett Unix-lösenord."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Underhållare"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Ditt lösenord har blivit ändrat."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Ändra lösenord"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Ange ditt nuvarande lösenord."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Konversation med \"passwd\" misslyckades."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Kunde inte hitta programmet \"passwd\"."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Fel lösenord! Försök igen."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr "Internt fel: otillåtet returnerat värde från PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Ange ditt nya lösenord."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Ditt lösenord kommer att bli nerkortat till 8 tecken."

