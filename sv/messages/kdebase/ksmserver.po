#: shutdown.cpp:76
msgid "<center><b><big><big>End KDE Session?</big></big></b></center>"
msgstr "<center><b><big><big>Avsluta KDE session?</big></big></b></center>"

#: shutdown.cpp:81
msgid "&Restore session when logging in next time"
msgstr "&Återställ session vid nästa inloggning"

#: shutdown.cpp:87
msgid "&Logout"
msgstr "&Logga ut"

#: main.cpp:21
msgid ""
"The reliable KDE session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"KDE:s pålitliga sessionshanterare som pratar det standardiserade X11R6 \n"
"sessionshanterings protokollet (XSMP)."

#: main.cpp:26
msgid "Restores the previous session if available"
msgstr "Återställer den föregående sessionen om tillgänglig"

#: main.cpp:28
msgid ""
"Starts 'wm' in case no other window manager is \n"
"participating in the session. Default is 'kwin'"
msgstr ""
"Startar 'wm' om ingen annan fönsterhanterare \n"
"deltar i sessionen. Standard är 'kwin'"

#: main.cpp:29
msgid "Also allow remote connections."
msgstr "Tillåt också fjärranslutningar."

#: main.cpp:42
msgid "The KDE Session Manager"
msgstr "KDE:s sessionshanterare"

