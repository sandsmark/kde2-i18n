#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"användning: %s [-alternativ] [meddelande ...]\n"
"\n"

#: options.cpp:225
msgid ""
"where options include:\n"
msgstr ""
"där alternativen är:\n"

#: options.cpp:226
msgid ""
"  -title name             the title used on the message window\n"
msgstr ""
"  -titel namn             den titel som används på meddelandefönstret\n"

#: options.cpp:227
msgid ""
"  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -file filnamn          fil att läsa meddelanden från, \"-\" för stdin\n"

#: options.cpp:228
msgid ""
"  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
"  -buttons sträng         komma-separerad lista av etikett[:slutkod]\n"

#: options.cpp:229
msgid ""
"  -default button         button to activate if Return is pressed\n"
msgstr ""
"  -default knapp         knapp att aktivera om returtangenten trycks ner\n"

#: options.cpp:230
msgid ""
"  -cancel button          button to activate if Escape is pressed\n"
msgstr ""
"  -cancel knapp          knapp att aktivera om Esc-tangenten trycks ner\n"

#: options.cpp:231
msgid ""
"  -print                  print the button label when selected\n"
msgstr ""
"  -print                  skriver ut knappetiketten när den väljs\n"

#: options.cpp:232
msgid ""
"  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -esacpe                 expandera \\n,\\r och \\t tecken i meddelandet\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -timeout sek            sluta med status 0 efter \"sek\" sekunder\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"de följande alternativen är tillåtna för kompatibilitet, men har ingen "
"funktion:\n"

#: options.cpp:236
msgid ""
"  -center\n"
msgstr ""
"  -center\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -nearmouse\n"
"\n"

