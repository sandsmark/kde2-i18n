#: xml_doc.cc:530
msgid "IMSQRT(\"1.2i\") returns \"0.774597+0.774597i\" "
msgstr "IMSQRT(\"1,2i\") returnerar \"0,774597+0,774597i\" "

#: xml_doc.cc:532
msgid "Power"
msgstr "Exponent"

#: xml_doc.cc:533
msgid "The IMPOWER(string) returns a complex number raised to a power."
msgstr "Funktionen IMPOWER(sträng;exp) upphöjer ett komplext tal till en exponent."

#: xml_doc.cc:534
