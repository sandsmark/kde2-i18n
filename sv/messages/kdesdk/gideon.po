msgid "<qt>Check calls to <i>printf()</i>, <i>scanf()</i> etc.</qt>"
msgstr "<qt>Kontrollera anrop till <i>printf()</i>, <i>scanf()</i> etc.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:335
msgid "<qt>Warn when a declaration does not specify a type.</qt>"
msgstr "<qt>Varna om en deklaration inte anger en typ.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:338
msgid "<qt>Issue a warning when a non-declared function is used.</qt>"
msgstr "<qt>Visa en varning om en icke-deklarerad funktion används.</qt>"

#: plugins/gccoptions/gccoptionsplugin.cpp:341
msgid "<qt>Issue an error when a non-declared function is used.</qt>"
msgstr "<qt>Ge fel om en icke-deklarerad funktion används.</qt>"

