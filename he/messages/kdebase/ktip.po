#: ktipwindow.cpp:61
msgid "Kandalf's useful tips"
msgstr "ףלאדנק לש תוישומישה תוצעה"

#: ktipwindow.cpp:98
msgid "Run on startup"
msgstr "הלעפהב גצה"

#: ktipwindow.cpp:244
msgid "Kandalf's tips"
msgstr "ףלאדנק לש תוצעה"

#: ktipwindow.cpp:248
msgid "KTip"
msgstr "KTip"

#: _translatorinfo.cpp:1
