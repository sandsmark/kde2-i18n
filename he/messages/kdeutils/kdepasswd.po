#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr ".הז שמתשמ לש המסיסה יוניש"

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr ".סקינוי תמסיס יוניש"

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "קזחתמ"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr ".התנוש ךלש המסיסה"

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "המסיס יוניש"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr ".ךלש תיחכונה המסיסה תא דלקה אנא"

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr ".הלשכנ `passwd' םע תורשקתהה"

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr ".`passwd' תינכותה תא אוצמל ןתינ אל"

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr ".תינש הסנ אנא !היוגש המסיס"

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
".PasswdProcess::checkCurrent-מ יקוח אל הרזחה ךרע "
":תימינפ האיגש"

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr ".ךלש השדחה המסיסה תא דלקה אנא"

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr ".םיוות 8 םומיסקמל רצוקת ךלש המסיסה"

