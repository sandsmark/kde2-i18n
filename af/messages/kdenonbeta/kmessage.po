#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"gebruik: %s [-options] [boodskap ...]\n"
"\n"

#: options.cpp:225
msgid "where options include:\n"
msgstr "waar opsies sluit in:\n"

#: options.cpp:226
msgid "  -title name             the title used on the message window\n"
msgstr "  -titel naam             die titel gebruik word op die boodskap venster\n"

#: options.cpp:227
msgid "  -file filename          file to read message from, \"-\" for stdin\n"
msgstr "  -lêer lêernaam          lêer aan lees boodskap van, \"-\" vir stdin\n"

#: options.cpp:228
msgid "  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr "  -buttons string         comma-separated lys van etiket[:uitgang kode]\n"

#: options.cpp:229
msgid "  -default button         button to activate if Return is pressed\n"
msgstr "  -default knoppie         knoppie aan aktiveer as terug is gedruk\n"

#: options.cpp:230
msgid "  -cancel button          button to activate if Escape is pressed\n"
msgstr "  -cancel knoppie          knoppie aan aktiveer as escape is gedruk\n"

#: options.cpp:231
msgid "  -print                  print the button label when selected\n"
msgstr "  -druk                  druk die knoppie etiket wanneer gekose\n"

#: options.cpp:232
msgid "  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr "  -escape                 vergroot \\n,\\r, en \\t karakters in boodskap\n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -timeout sekondes           beïendig met status 0 na \"sekondes\" sekondes\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"die volgende opsies word toegelaat vir aanpasbaarheid, maar word "
"non-functional:\n"

#: options.cpp:236
msgid "  -center\n"
msgstr "  -center\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -nearmouse\n"
"\n"

