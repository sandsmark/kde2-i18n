#: shutdown.cpp:76
msgid "<center><b><big><big>End KDE Session?</big></big></b></center>"
msgstr "<center><b><big><big>einde kde sessie?</big></big></b></center>"

#: shutdown.cpp:81
msgid "&Restore session when logging in next time"
msgstr "herstel sessie wanneer afteken in volgende tyd"

#: shutdown.cpp:87
msgid "&Logout"
msgstr "teken af"

#: main.cpp:21
msgid ""
"The reliable KDE session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"die betroubaar kde sessie bestuurder wat praat die standaard x11r6 \n"
"sessie bestuuring protokol (xsmp)."

#: main.cpp:26
msgid "Restores the previous session if available"
msgstr "herstel die vorige sessie as beskikbaar"

#: main.cpp:28
msgid ""
"Starts 'wm' in case no other window manager is \n"
"participating in the session. Default is 'kwin'"
msgstr ""
"beginne 'wm' in kas nee ander venster bestuurder is \n"
"neem deel in die sessie. verstek is 'kwin'"

#: main.cpp:29
msgid "Also allow remote connections."
msgstr "asook toelaat afgeleë koppelinge."

#: main.cpp:42
msgid "The KDE Session Manager"
msgstr "die kde sessie bestuurder"

