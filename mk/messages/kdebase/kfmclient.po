#: kfmclient.cc:54
msgid "KDE tool for opening URLs from the command line"
msgstr ""
"КДЕ алатка за отворање УРЛ-а од "
"командната линија"

#: kfmclient.cc:60
msgid "Show available commands."
msgstr "Покажи достапни команди."

#: kfmclient.cc:61
msgid "Command (see --commands)."
msgstr "Команда (видете --commands)."

#: kfmclient.cc:62
msgid "Arguments for command."
msgstr "Аргименти за командата."

#: kfmclient.cc:76
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"Синтакса:\n"

#: kfmclient.cc:77
#, fuzzy
msgid ""
"  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n"
"\n"
msgstr ""
"  kfmclient openURL 'url'\n"
"            # Отвора прозор за приказ на 'url'.\n"
"            #  'url' може да биде релативен пат или "
"име на\n"
"            #   датототека, на пример . или "
"папка/\n"
"            #   Ако се изостави 'url', се користи "
"$HOME.\n"
"\n"

#: kfmclient.cc:82
msgid ""
"            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that konqueror should use. For instance, set it "
"to\n"
"            #   text/html for a web page, to make it appear faster\n"
"\n"
msgstr ""

#: kfmclient.cc:86
#, fuzzy
msgid ""
"  kfmclient openProfile 'profile' ['url']\n"
"            # Opens a window using the given profile.\n"
"            #   'profile' is a file under "
"~/.kde/share/apps/konqueror/profiles.\n"
"            #   'url' is an optional URL to open.\n"
"\n"
msgstr ""
"  kfmclient openProfile 'profile'\n"
"            # Отвора прозор со зададениот "
"профил.\n"
"            #   'профил' е датотека во папката "
"~/.kde/share/apps/konqueror/profiles.\n"
"\n"

#: kfmclient.cc:91
msgid ""
"  kfmclient openProperties 'url'\n"
"            # Opens a properties menu\n"
"\n"
msgstr ""
"  kfmclient openProperties 'url'\n"
"            # Отвора мени со поставки\n"
"\n"

#: kfmclient.cc:93
msgid ""
"  kfmclient exec ['url' ['binding']]\n"
"            # Tries to execute 'url'. 'url' may be a usual\n"
"            #   URL, this URL will be opened. You may omit\n"
"            #   'binding'. In this case the default binding\n"
msgstr ""
"  kfmclient exec ['url' ['поврзување']]\n"
"            # Го извршува 'url'. 'url' може да биде "
"обичен\n"
"            #   URL и тој ќе биде отворен. Можете "
"да испуштите\n"
"            #   'поврзување' и во тој случај ќе се "
"користи вообичаеното\n"

#: kfmclient.cc:97
msgid ""
"            #   is tried. Of course URL may be the URL of a\n"
"            #   document, or it may be a *.desktop file.\n"
msgstr ""
"            #   поврзување. Секако, URL може да "
"биде URL на\n"
"            #   документ или може да биде *.desktop "
"датотека.\n"

#: kfmclient.cc:99
msgid ""
"            #   This way you could for example mount a device\n"
"            #   by passing 'Mount default' as binding to \n"
"            #   'cdrom.desktop'\n"
"\n"
msgstr ""
"            #   На тој начин, на пример, можете да "
"монтирате уред\n"
"            #   со давање на 'Mount default' како "
"поврзување на \n"
"            #   'cdrom.desktop'\n"
"\n"

#: kfmclient.cc:102
msgid ""
"  kfmclient move 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
msgstr ""
"  kfmclient move 'извор' 'цел'\n"
"            # Го преместува 'извор' во 'цел'.\n"
"            #   'извор' може да биде и листа од URL "
"адреси.\n"

#: kfmclient.cc:107
msgid ""
"  kfmclient copy 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"\n"
msgstr ""
"  kfmclient copy 'извор' 'цел'\n"
"            # Го копира 'извор' во 'цел'.\n"
"            #   'извор' може да биде и листа од URL "
"адреси.\n"
"\n"

#: kfmclient.cc:110
msgid ""
"  kfmclient sortDesktop\n"
"            # Rearranges all icons on the desktop.\n"
"\n"
msgstr ""
"  kfmclient sortDesktop\n"
"            # Ги подредува сите икони на "
"работната површина.\n"
"\n"

#: kfmclient.cc:112
msgid ""
"  kfmclient configure\n"
"            # Re-read konqueror's configuration.\n"
"\n"
msgstr ""
"  kfmclient configure\n"
"            # Ја превчитува конфигурацијата на "
"konqueror.\n"
"\n"

#: kfmclient.cc:114
msgid ""
"  kfmclient configureDesktop\n"
"            # Re-read kdesktop's configuration.\n"
"\n"
msgstr ""
"  kfmclient configureDesktop\n"
"            # Ја превчитува конфигурацијата на "
"kdesktop.\n"
"\n"

#: kfmclient.cc:117
msgid ""
"*** Examples:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Mounts the CDROM\n"
"\n"
msgstr ""
"*** Примери:\n"
"  kfmclient exec file:/root/Desktop/cdrom.desktop \"Mount default\"\n"
"             // Го монтира цедеромот\n"
"\n"

#: kfmclient.cc:120
msgid ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with default binding\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html\n"
"             // Ја отвора датотеката со "
"вообичаени поврзувања\n"
"\n"

#: kfmclient.cc:122
msgid ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Opens the file with netscape\n"
"\n"
msgstr ""
"  kfmclient exec file:/home/weis/data/test.html Netscape\n"
"             // Ја отвора датотеката со netscape\n"
"\n"

#: kfmclient.cc:124
msgid ""
"  kfmclient exec ftp://localhost/\n"
"             // Opens new window with URL\n"
"\n"
msgstr ""
"  kfmclient exec ftp://localhost/\n"
"             // Отвора нов прозор со URL\n"
"\n"

#: kfmclient.cc:126
msgid ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/emacs.desktop\n"
"             // Пушта emacs\n"
"\n"

#: kfmclient.cc:128
msgid ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Opens the CD-ROM's mount directory\n"
"\n"
msgstr ""
"  kfmclient exec file:/root/Desktop/cdrom.desktop\n"
"             // Ја отвора папката каде што е "
"монтиран цедеромот\n"
"\n"

#: kfmclient.cc:130
msgid ""
"  kfmclient exec .\n"
"             // Opens the current directory. Very convenient.\n"
"\n"
msgstr ""
"  kfmclient exec .\n"
"             // Ја отвора тековната папка. Многу "
"згодно.\n"
"\n"

#: kfmclient.cc:254
msgid ""
"Profile %1 not found\n"
msgstr ""
"Профилот %1 не е најден\n"

