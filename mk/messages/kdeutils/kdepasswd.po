#: kdepasswd.cpp:25
msgid "Change password of this user."
msgstr "Смени лозинка на овој корисник."

#: kdepasswd.cpp:32
msgid "KDE passwd"
msgstr "KDE passwd"

#: kdepasswd.cpp:33
msgid "Changes a Unix password."
msgstr "Менува Unix лозинка."

#: kdepasswd.cpp:35
msgid "Maintainer"
msgstr "Одржувач"

#: kdepasswd.cpp:57
msgid "Your password has been changed."
msgstr "Вашата лозинка е сменета."

#: passwddlg.cpp:23 passwddlg.cpp:83
msgid "Change password"
msgstr "Смени лозинка"

#: passwddlg.cpp:24
msgid "Please enter your current password."
msgstr "Внесете ја тековната лозинка."

#: passwddlg.cpp:41 passwddlg.cpp:121
msgid "Conversation with `passwd' failed."
msgstr "Неуспешна конверзација со `passwd'."

#: passwddlg.cpp:49
msgid "Could not find the program `passwd'."
msgstr "Не може да се пронајде програмата `passwd'."

#: passwddlg.cpp:53
msgid "Incorrect password! Please try again."
msgstr "Погрешна лозинка! Обидете се повторно."

#: passwddlg.cpp:57
msgid "Internal error: illegal return value from PasswdProcess::checkCurrent."
msgstr ""
"Внатрешна грешка: недозволена повратна "
"вредност од PasswdProcess::checkCurrent."

#: passwddlg.cpp:84
msgid "Please enter your new password."
msgstr "Внесете ја новата лозинка."

#: passwddlg.cpp:100
msgid "Your password will be truncated to 8 characters."
msgstr "Вашата лозинка ќе биде скратена на 8 знаци."

