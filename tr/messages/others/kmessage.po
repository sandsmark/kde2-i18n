#: options.cpp:223
#, c-format
msgid "usage: %s [-options] [message ...]\n\n"
msgstr ""
"kullanım şekli: %s [-seçenekler] [mesaj ...]\n"
"\n"

#: options.cpp:225
msgid "where options include:\n"
msgstr "seçenekler şunları içerir:\n"

#: options.cpp:226
msgid "  -title name             the title used on the message window\n"
msgstr "  -title başlık          mesaj penceresinde kullanılacak başlık\n"

#: options.cpp:227
msgid "  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -file dosya-adı          mesajın alınacağı dosya adı, stdin için \"-\" \n"

#: options.cpp:228
msgid "  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
"  -buttons yazı          etiketlerin virgüller ile ayrılmış "
"listesi[:çıkışkodu]\n"

#: options.cpp:229
msgid "  -default button         button to activate if Return is pressed\n"
msgstr "  -default düğme          Return'e basıldıysa aktif olcak düğme\n"

#: options.cpp:230
msgid "  -cancel button          button to activate if Escape is pressed\n"
msgstr "  -cancel düğme          Escape'e basıldıysa aktif olacak düğme\n"

#: options.cpp:231
msgid "  -print                  print the button label when selected\n"
msgstr "  -print                seçildiğinde düğmenin etiketini yaz\n"

#: options.cpp:232
msgid "  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -escape                 mesajdaki \\n,\\r ve \\t karakterlerini "
"kısaltmadan tam olarak yaz\n"

#: options.cpp:233
msgid "  -timeout secs           exit with status 0 after \"secs\" seconds\n\n"
msgstr ""
"  -timeout saniye         \"saniye\" sonra 0 durumu ile çık\n"
"\n"

#: options.cpp:235
msgid "the following options are allowed for compatibility, but are non-functional:\n"
msgstr ""
"bundan sonraki seçenekler uyumluluk içindir, fakat fonksyonel değillerdir:\n"

#: options.cpp:236
msgid "  -center\n"
msgstr "  -center\n"

#: options.cpp:237
msgid "  -nearmouse\n\n"
msgstr ""
"  -nearmouse\n"
"\n"

