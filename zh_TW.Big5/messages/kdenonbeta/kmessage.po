#: options.cpp:223
#, c-format
msgid ""
"usage: %s [-options] [message ...]\n"
"\n"
msgstr ""
"用法： %s [-選項] [訊息…]\n"
"\n"

#: options.cpp:225
msgid ""
"where options include:\n"
msgstr ""
"哪裡包含選項：\n"

#: options.cpp:226
msgid ""
"  -title name             the title used on the message window\n"
msgstr ""
"  -標題名稱             用在訊息視窗的標題\n"

#: options.cpp:227
msgid ""
"  -file filename          file to read message from, \"-\" for stdin\n"
msgstr ""
"  -檔案名稱          從檔案中讀取訊息， \"-\" 標準輸入\n"

#: options.cpp:228
msgid ""
"  -buttons string         comma-separated list of label[:exitcode]\n"
msgstr ""
"  -按鈕字串         以逗號隔開的標簽列表[:結束碼]\n"

#: options.cpp:229
msgid ""
"  -default button         button to activate if Return is pressed\n"
msgstr ""
"  -預設按鈕         如果回傳按鈕按下欲啟動的按鈕\n"

#: options.cpp:230
msgid ""
"  -cancel button          button to activate if Escape is pressed\n"
msgstr ""
"  -取消按鈕          如果跳脫按鈕按下欲啟動的按鈕\n"

#: options.cpp:231
msgid ""
"  -print                  print the button label when selected\n"
msgstr ""
"  -列印                  選擇時列印按鈕標簽\n"

#: options.cpp:232
msgid ""
"  -escape                 expand \\n,\\r, and \\t characters in message\n"
msgstr ""
"  -跳脫                 訊息中的擴充字元 \\n,\\r, and \\t \n"

#: options.cpp:233
msgid ""
"  -timeout secs           exit with status 0 after \"secs\" seconds\n"
"\n"
msgstr ""
"  -逾時秒數           在 \"secs\" 秒後就結束並回傳狀態 0\n"
"\n"

#: options.cpp:235
msgid ""
"the following options are allowed for compatibility, but are "
"non-functional:\n"
msgstr ""
"下列的選項為了相容性而允許，但沒有功能性：\n"

#: options.cpp:236
msgid ""
"  -center\n"
msgstr ""
"  -中間\n"

#: options.cpp:237
msgid ""
"  -nearmouse\n"
"\n"
msgstr ""
"  -nearmouse\n"
"\n"

